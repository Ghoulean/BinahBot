<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Binah</GroupName>
  <Character ID="B_E_Elena">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">どれどれ……味わえる血管が残ってるかな？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">渇きの苦痛に耐えるのは……美しいことじゃない。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">満足のいく食事では無かったな。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">この渇きから……逃れる方法は……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">空っぽの殻だけだな。飲める血が１滴すら残っていない……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">死は公平だ……皆一緒に踊りを踊るから。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">食って食われる運命もまた、別け隔てなく私たちを照らすから。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">惜しいけど……私が望む味ではなかったな。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">お前たちも欲望を満たすために杯をあけるんだろ？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">あぁ……そんなので私を止められるとでも？</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Orange">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Binah">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">あまり念慮するな。今は集中する方が善いな。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">此方は終わった様だな。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">お前の力量が不足しただけよ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">其れでもかなり永く耐えて呉れたな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">きっと出せる力を尽くしたろうに。ご苦労さま。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">誰もお前を叱責したりはしないよ。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">軽い身体でも安らかに横たわらんことを願おう。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">どうか今度こそは永久にお眠りなさい。</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">とにかく……ここを尋ねて来たゲストなんだし着実に接待しないと。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">お疲れさまでした……他の階が心配ですね。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">こんな風に引き下がるわけには……！</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">最後まで抗ってみせる……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">１人でも残っているなら終わりじゃない。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">焦っちゃ駄目だ……。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">もうちょっと楽になりそう……。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">あの血鬼も本に…なったのか？</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">特色を使役するだなんて…大丈夫ですかね……？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">皆さん無事ですか？何かあったら言ってください。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">ごめんなさい…これ以上耐えられません……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">残った者はより慎重</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">今まで努力してきたことを水の泡にすることは出来ません……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">まだ……大丈夫です……。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">少しでも遅れたら大変なことになるところだった……。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">血鬼たちの力は…みんなこの程度なのか？</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">朱色……十字。間違いない。あの巨大な十字架……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……なんか落ち着かないなぁ。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">まだ残ってるのに……まだ……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……油断したか。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">無闇に動くな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">ここまで来て立ち止まるわけにはいかないだろ。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">抜け殻でも力が無くなったわけじゃないのか。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">なんだか目眩がするなぁ。</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……隣りにいるのは誰だろう。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">疲れた…そろそろ整理して帰りますか。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">やられ……ちゃった……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">他に方法がありますか……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">人数が少ないからって舐めて掛かるべき相手じゃなかった……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">どうすればこの状況をひっくり返せるのでしょうか。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">噂に聞く特色を倒す日が来るだなんて……。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">本当に……普通の人が持ち得る力じゃないみたいですね。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>