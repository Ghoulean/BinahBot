<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Malkuth</GroupName>
  <Character ID="M_P_Philip">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">これ以上話すこともないでしょう。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">……悪い感情を抱いているわけではありません。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">見てください。昔の僕と同じ姿じゃないですか。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">ただ、帰るだけ……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">…………。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">僕と違ってあなたたちは何も変わってません。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">いつか自分のそばを離れることに未練が無いことを……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Malkuth">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">緊張せずに落ち着いてやってこう！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">なんだかこう、そんなにスッキリしないね。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……諦めないで。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">大丈夫。自分のせいにしないで。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">きっと出来るよ……信じてもいいよ！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">これ以上引き下がる場所はない……最後まで立ち向かおう！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">十分上手くやれてるね！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">よし……このまま前へ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">まだまだ余裕だね？</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">マルクトさん！これからどうしましょう……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">終わりました。……片付けて戻りましょう。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">ううん…もっと役に立たないと……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">怖くなってきた……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">押されてる。もう少しだけ考えてみよう……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">何が何でもぶつかるしかないのか……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">あの天使の彫刻、まだあるんですね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">早く終わらせないと……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">皆さん大丈夫ですか？</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">沢山変わりました。私たちも、あいつも。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">なんだか地味に後味悪いですね。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">クソっ…こういう風に終わっちゃ……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">より危なくなるな……気を付けないと。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">私たちだけで……止められるよね？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">一体どんな楽団ならあんな力が……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">これ以上の時間稼ぎは無しだ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">一度は会ったことある相手だから……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">なんだか上手くいってるみたいだな？</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">暴れまわってたときよりかは相手しづらくなってますよね……？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">はぁ……楽な相手じゃなかったですね……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">荷物になりたくはない……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……やり方を変えないといけませんね。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">このままじゃこの階が……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">どうしよう……どうしたら……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">まだ余裕満々ですね！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">このままいけば大丈夫ですよ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">このまますぐサポートに回ります！</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">前とは違って落ち着いてるみたいだね。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">これで勝ちで良いのかな……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">もっと……耐えなきゃ……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">心配しないで。問題ないから。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">ごめんね。背中を守ってあげなきゃだめだったのに。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……残りは任せて。.</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">やり方がそこまで変わったわけではないかな……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">またあの彫刻か……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">楽団ってやつらの力が全部この程度なら……。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>