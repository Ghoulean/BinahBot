<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Tomerry</GroupName>
  <Character ID="Tomerry">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">ほんをもってかえればごほうびをくれるっていわれました。どんなごほうびだろう！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">わたしたちのまちよりひろいみたいですね……おかあさんみたいにちいさいひともたくさん……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">いいこはどんなことにもいいわけしないでやるんです！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">ここにあるほんをいっぱいもってかえればきっとよろこんでくれますよね？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">おかあさんだけじゃなくて、こんどはおとうさんからもほめてもらえそうです！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……こころぼそいわ。わたしがきえたみたい。かなしいわ……。なみだがでそう……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">まちをいじめるわるいやつらよりもっとつよいきがします……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">わたしのともだちが……とてもいたいいたいです……どうしましょう……？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">あっ……お、おかあさんにおしおきされるかもしれません……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">まちのみんなとへいわをまもりにきました！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">こうやってがんばれば……おかあさんがほめてくれるよね？たのしいな……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">いっしょにあそびたいんですか？わたしもだいすきでたのしいです！</BattleDialog>
    </Type>
  </Character>
  <Character ID="Tomerry_Phase2">
    <Type ID="START_BATTLE" />
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">ひとりじゃさびしくてたえられないの……。わたしはどこにいったの？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……どこなの。とてもこわいわ……。そばにきて。トミー……トミー。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">… …</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">あいのちからをしらないなら……りかいしなさい。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">わたしをくるしめた、ばつよ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">……あいするおかあさんのために。</BattleDialog>
    </Type>
  </Character>
  <Character ID="LoveTownPeople">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">なぐってまた… なぐﾙ…ﾝだ……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">さっｻと……ほしがたにおｯ…てやっつｹよう……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">わる…ｲやつらにはきﾋﾞしい…ばつがいるよね……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">ﾐんなの……ﾀ…めにｶﾞんばﾙよ……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">いたいのわいやなのに……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_5">ﾐんなのちｶらに……なるんだ……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_6">むりするﾅってお…ｶあさんが……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_7">あいはｲ……いものだ……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_8">ﾀﾞいじｮおぶ……ぼくらのあいﾅら……かﾃる……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">ごほうびﾓらいにいくんだ……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">こﾝどはどう……んﾅごほうびがﾓらえるｶな……。トマリーみたいなおおきな……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">だれにも……まけﾅい……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">これがまさにあｲ……のﾁからﾀﾞ……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">しﾌﾞといあいのﾁからをなめるな……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">ｼにﾀ……くﾅい……。</BattleDialog>
      <BattleDialog ID="DEATH_1">あいするﾏちのﾐんなといっ…ｼょにいﾀい……。</BattleDialog>
      <BattleDialog ID="DEATH_2">ﾔ…らﾙえ…ﾀ……。</BattleDialog>
      <BattleDialog ID="DEATH_3">こわｲです……ﾀ……すｹてください……。</BattleDialog>
      <BattleDialog ID="DEATH_4">ごめ… ﾅさ… ｨ… ﾓうしまｾ…ﾝから……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">ひとりﾆなっﾂぁいそうだ……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">くﾙしむのはｲや……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">みﾝなあいをｼればｲいのに……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">まﾁのみﾝ……なおいじﾒないでください……おねがい……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">に…にげなきゃいけなｲんじゃ……？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">そ…ｯくりそのままなぐﾘかえｽんだ……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">だｨじょﾌﾞ……あれ……くらｨわく…ﾗわないｶら……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">あいつわ……たおｼた……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">みんﾅといっｼょならい……ｧれる……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">ま……ﾀﾞもっとたﾀかえる……。</BattleDialog>
    </Type>
