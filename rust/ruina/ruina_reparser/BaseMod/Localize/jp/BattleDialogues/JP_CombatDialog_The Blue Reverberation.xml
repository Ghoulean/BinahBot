    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">俺が期待したのはこんな赤い霧じゃなかった。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Argalia_MeetRoland">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">イカれた後にどこに行ったと思いきや……ここにいたんだ。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">あのとき出来なかった話をしようか。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">本はありがたく持ってくね。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">君たちが完璧に準備した後に……また会おうね。</BattleDialog>
    </Type>
