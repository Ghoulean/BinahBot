<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Tiphereth</GroupName>
  <Character ID="T_O_Oswald">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">おほんほん！公演を始めますよぉ～。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">さあさあ！終わりのない笑いと拍手が聞こえるように！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">最後まで付き合っていただいた道化師さんたちに……どうか盛大な拍手を！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">私だけのスポットライトだったのに……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">ま、まさか……わが団員が……！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">どうして笑いを与えたいだけの我々を！！！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">うぅぅん、道化師お一方がと～っても面白がったみたいですねぇ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">床を転げ回りたいくらい楽しいですよね？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">やっぱり！私のショーは皆を笑わせることができます。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">もっと大きな声で歌を歌って差し上げましょう！</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Tiphereth">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">大丈夫。ずっと今まで通りに。分かってるよね？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……みんなお疲れ様。片付けてから待ってようか。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……いいえ。きっとやり遂げられる。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">立ち止まるにはまだ早いでしょ！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">お互いを信じて前に進めばきっと……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">これ以上あんたたちの思い通りにはさせない……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">しっかりして。まだ希望があるから。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">大変だとは思うけど、もう少しだけ押していこう！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">なんとか上手くやれてるから……自分のこと心配してよ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">悪くないね。まだみんな疲れ切ってはいないでしょ？</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">初っ端から怖じ気ついてませんよね？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">うーん……他の連中も上手いことやってますよね？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">ごめん……もう限界だ……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">残りは任せてもらって大丈夫だから。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">こんなことで崩れたりはしない。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……より全力を尽くさないとね！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">こっちは終わりました～。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">ややこしいけど……興味深い方々でしたね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">みなさん大丈夫ですよね？</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">やるしか……ないんだよね。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">無事終わってよかった。ね？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">もっとお手伝いできると思ったのに……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">こんなときこそ……より冷静に。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">見た目もやることも気持ち悪いなぁ……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">他の人は耐えられるかなぁ？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">私も道化師になった気分になるなぁ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">私の分はなんとか終わらせられたかなぁ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">あの歌声のせいでまだ頭がガンガン響いてる……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">なんだかサーカスに来た気がするなぁ……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">笑いを与える公演かぁ……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">なんだか……道化師の手のひらの上で踊らされた気が……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">もっと良い方法があるはず……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">ちょっと不安だけど……君の分まで戦ってあげる。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">舐めて掛かっちゃ駄目だろうな。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">よし。このまま突き進もう！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">1つの……公演をしてる気になるなぁ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">気持ち悪い見た目してるなぁ……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">前会ったつらと似てるなぁ。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">ティファレトさん。帰りますか？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">変に振り回されて面倒事を増やすなよ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……無理もないか。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">後もう少しだから……もうちょっとだけ耐えて。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">こんなうるさい公演は好きじゃないなぁ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">あの道化師、口だけは達者だなぁ……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">公演に付き合うつもりもない。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>