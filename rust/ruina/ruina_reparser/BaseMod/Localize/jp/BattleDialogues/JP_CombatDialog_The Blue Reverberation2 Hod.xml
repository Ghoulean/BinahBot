<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Hod</GroupName>
  <Character ID="H_G_Greta">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">キャハハ！！じゃあ下ごしらえを始めようか？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">凄く興味深い材料だね！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">他のやつらは上手いことやってるのかなぁ！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">これ…最高の珍味に出会えたね……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">そんなぁ……料理できないの？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">楽勝だね！一口サイズのやつらばっかりね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">後味スッキリしないねぇ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">新しいレシピがいっぱい浮かんでくるね！</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Hod">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">私も長いことあなたたちを見ながら色んなことを学んだから。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……それでも少しでも力になれたなら。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">私はまだ大丈夫だけど……あなたたちはどう？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……良い方法を見つけないと。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">う、うん。大丈夫。立ち止まったりはしない！</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">あなたの夢を踏みにじるつもりはなかったんです。ただ……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">ホドさん。大丈夫ですか？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……自業自得です。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">だめだ……軽率に動くほど不利になる。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">完全に窮地に追い込まれたわけではありません。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">動きを読まないと……。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……山場は越えたみたいですね。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">他人の心配をしていられる立場ではないですよね。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">何の役にも立て…なかったのに……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">冷静に……進んでいくしかありません。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">乱暴極まりないですね……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">状況はそこまで悪くないです……多分……。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">何とか終わらせられましたね……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">他の階が心配ですね…大丈夫かなぁ……？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">出来る限り耐えられたとは……思ってます。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">ごめんなさい。今はできることが……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">油断したらすぐ……やられる……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">状況はそこまで傾いてはいません……。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">どうにか止められましたね……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">口何個あるんだよ……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">クソッ……油断した……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">慎重に見て避けないと……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">あの鮫のやつ……手のつけようなくないか？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">本当に食材って思って殺してるのか……。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">はぁ……あの重い武器はちょっと物騒だったな。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>