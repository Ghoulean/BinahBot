  <Character ID="R_Corp_Rudolph_2_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">馴鹿たち、早くボコして出ていこうか。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">もう既に経験しただろ？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">戦い方が明け透けだな。一気に勝てたよ。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">今度も押されるのは危ないのに……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">危なくなってきた…早く終わらせないといけないのに……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">全ては精神力の戦いだ……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">前と同じだと思って油断したみたいだね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">馴鹿たち。残りは少ない。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">急げ！長引いたら不利になるのは僕たちだ。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Reindeer_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">はぁ……また出撃か。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">今度は確実に踏み潰せるよな……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不安になっても無駄だよ。どうしようもないからな。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">まぁ、生きるために頑張るか。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">一気に終わらせようか。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">もう少しでも時間を稼がれたら危なかった。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">これで殺処分は免除されるのか？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">知らないよ。上で良い感じに処理してくれるだろ。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">ルドルフさん、帰りますか？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">今度は勝てると思ったのに……。</BattleDialog>
      <BattleDialog ID="DEATH_1">残ったやつらが…なんとか頑張ってくれ……。</BattleDialog>
      <BattleDialog ID="DEATH_2">これで本当に終わりか……？</BattleDialog>
      <BattleDialog ID="DEATH_3">この化け物を相手に勝てるわけが……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">耐えなければ…みんな耐えよう。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">前にもこんな状況になった気が……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">焦ってはいけないと分かっているのに……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">一体……こんな力はどこから湧いてくるのか……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">良い武器もまともに使えなければ意味がないんだ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">まだ問題ないから戦い続けるか。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">…耐えづらくなってる。急いで終わらせないと……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">他のやつらも段々疲れていってるみたいだな。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Maxim_2_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">今度は生きて帰るぞ！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">今は暴れることが俺たちの任務だ！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">細かいことはあいつらを倒した後だ！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">ははっ！俺たちのことを見くびって油断したみたいだな！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">あと何匹だ？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">やっぱり厄介なやつらだな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">おっとぉ！まともに踏み潰されたか。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">力で押しつぶせばテクニックも何も無駄だ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">突進すれば勝手に道が開かれるからな！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">サイの角のように独りで征け！</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Rhinoceros_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">迷わず果敢に！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">どんな敵であれ、サイの足に押しつぶされるだけだ！ </BattleDialog>
      <BattleDialog ID="START_BATTLE_2">誰から潰そうか！</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">サイの前を塞げるものは何もない！</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">隊長。今すぐ始めれば良いんですか？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">勝利だ！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">今度も勝てると思ったか？惜しいな！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">これで殺処分されるのは保留だな。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">面白いやつらだな！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">これくらいで折れたりはしない……！</BattleDialog>
      <BattleDialog ID="DEATH_1">…かはっ…耐えなければ……！</BattleDialog>
      <BattleDialog ID="DEATH_2">このまま倒れたりは……！</BattleDialog>
      <BattleDialog ID="DEATH_3">クソッ…何も変わらなかったな……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">もうちょっとズレてたらこっちが殺られてたな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">いつものことだ！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">群れの全滅に繋がらないと良いな！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">気にしない！やるべきことがあるから！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">身体が凄くか弱いな、だろ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">そんな攻撃は無駄だ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">まだ全部終わったわけではない！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">前に立ちはだかるとは……愚かだな！ははっ！</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Myo_2_1">
