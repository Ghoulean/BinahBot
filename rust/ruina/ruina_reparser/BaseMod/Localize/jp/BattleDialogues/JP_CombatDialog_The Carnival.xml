<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Carnival</GroupName>
  <Character ID="Alpha">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">始めます？弱そうには見えません。気をつけます？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">美味しそうです？でも人間ではないです。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">あなた達も掃除屋みたいに不味いです？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">本を食べます？もっと素敵な糸が出てきます？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">早く帰らないといけません？指令がありますから。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……あまりいい味じゃありませんでした。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">出過ぎました？やられました。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">悲しいです？一緒にいた友だちが死にました。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">まだ危なくはないです？大丈夫です。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">攻撃が単純です。簡単です?</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">早く消しましょう。面倒になります？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">こんな小さな人にやられそうですか。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Beta">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">今が好機です。刺せそうですか?</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">強そうです? でも問題はありません。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">どんな味か気になります？固いけど美味しそうです。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">楽しいです？勝ちました。気分がとても浮き浮きしますか？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">私たちは強いですから？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">これ以上布を織ることはできないのですか？</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">誰かがやられました。方法を考える必要があります？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">このままだと私たちが負けます？気をつけてください。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">慎重になりましょう？大変なことになるかもしれません。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">本を食べればまた違った糸が出てきます？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">そんな力では私の身体に傷もつけられませんよ？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">まだ終わってません。どれだけ残っていますか？</BattleDialog>
    </Type>
  </Character>
  <Character ID="Gamma">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">苦しめるのは嫌です？殴られてばかりはいられません。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">新しい糸のためにはやらないといけません？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不必要な行動は減らします。時間との戦いですから？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">大きな被害はないです？でも疲れました。休みたいです？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">人差し指に渡す仕事だけが残っています。早く行きましょう？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">こんなはずないです？まだやりきれていないことがあります……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">本に変わりました？死ねばああいう風に変わります。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">大丈夫です。このままでは私たちが酷い目に遭うこともあります？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">むやみに出るのは危険です？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">１匹捕まえました？きれいに終わらせました。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">私たちが勝ちそうです？当たり前です。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">１つ片付けました。油断大敵です？</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>