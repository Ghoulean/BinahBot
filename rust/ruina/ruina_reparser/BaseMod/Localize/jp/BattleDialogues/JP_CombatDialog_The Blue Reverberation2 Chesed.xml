  <Character ID="C_J_Chesed">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">大丈夫～そんな緊張しないほうが良いと思うよ。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">操り糸が全て解かれた自由な世界かぁ……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">やっぱり俺の力じゃ難しかったみたいだね……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">焦らないで。まだ出来るって思うんだ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">無意味な犠牲ではなかったはずだ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……俺たちじゃなきゃ止められないだろうね。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">疲れたならちょっと下がっててもいいよ。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">順調に進んでるみたいだねぇ～。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">余裕こいてる暇はないか……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">みんな。もうちょっとだけ頑張ろ～。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">もうちょっとで片が付きそうだね～。</BattleDialog>
    </Type>
  </Character>
  <Character ID="C_J_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">上手く…やり遂げられますかね。不安なんです。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">どうにかやり遂げられた……みたいですね？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">他の方たちが…きっと…最後まで……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">いつになったらこういうことが終わるんですかね。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">軽く黙祷だけしていきますね。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">きっと大丈夫…です。絶対に止められますから。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">結構苦戦したけど……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">早く終わらせましょう。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">こっちは終わったので手伝いますね。</BattleDialog>
    </Type>
  </Character>
  <Character ID="C_J_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">皆であいつらを止めればいいってことだよね？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">ほ、本当に終わったか？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">少なくとも足を引っ張りは……しなかった。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">余裕がなさすぎても駄目だ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">こっちも気を引き締めないとな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">とりあえず戦いに集中すべきか。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">……まともに当たった！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">完全に壊れちゃったか……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">操作される人形なんかにやられるわけ……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="C_J_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">あの音聞くだけでも鳥肌立つよ。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">人形師を殺すことが人形に自由を与えることになるのかな。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">クソッ……もう動けない……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">どうしてこう……なったんだ？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">ケセドさん……これからどうすれば……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">やっぱり無謀だったんじゃないかな……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">それなりに楽じゃない？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">糸が切れたら動けないな。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">あんなやつらみたいにはなりたくない……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="C_J_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……あのおぞましい人形がまた来たんだ。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">ふぅ……操るやつは別に強くもなかったな。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">無事……終わると良いんだけど……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……より強気に出られる前に叩こう。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">十分責任を果たした。それだけだよ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">その分頑張らないと……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">糸を切って……あげられてるのかな？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">ある意味すごく可哀想だな。あの人形ってやつ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">……よし！あともう少しだ。</BattleDialog>
    </Type>
  </Character>
