  <GroupName>The Blue Reverberation2 Chesed</GroupName>
  <Character ID="C_J_Jae-heon">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">たとえ今は縛られていようが、俺がお前たちの糸を全て切ってやる。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">……じゃあ、人形遊びを始めてみようか。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">俺たちは必ずその世界に到れるはずだ。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">結局…糸は解けなかったな……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">これまた。こんなに暴れまわっちゃ……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">人形たちが全部壊れそうだな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">一緒に遊びに付き合ってくれたらいいんだけどな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">あの人形はもう使えなさそうだな……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">人形にできなかったのは本当に惜しいな。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">お前たちも糸を切って自由を得たくはないか？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">……人形たち。あと少しだ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">聞いてたのよりも弱いな。</BattleDialog>
    </Type>
  </Character>
