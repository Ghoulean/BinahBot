<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Sweepers</GroupName>
  <Character ID="Anton">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">掃除を…開始…するん…です…。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">止まらず…前に…進みます…。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">ただ…良さそうな…空間では…ないみたいです…。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">これで…家族全員が…まともに…食事が…できるで…しょう…。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">隣人たちが…頑張らないといけない…んですけど…。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">やるべきことは…早くこなすべきです…まだ…あんなにあります…。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">隣で…誰が倒れようと…進みます…。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">私はまだ…大丈夫です…。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">食べ…られ…ません…。本になりました…。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">掃除じゃない時間に…掃除するのは…久しぶりです…。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">前に…あるものだけ…しっかり掃除すれば良いんです…。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Valerie">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">隣人の皆さん、燃料は満タンにしてきましたか？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">家族とならどんな苦難にも耐えることができます。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">子供たちのためにもここで本というものを手に入れなければなりません。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">そこに落ちている本を回収しましょう。母がきっと喜ばれます。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">隣人の皆さん…気をつけて…ください…。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">町に残っている家族のためにも立ち止まれません。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">前にいるものを先に全て一掃する必要があります。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……隣人さんが死なれました。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">いつも通り食事をするだけです。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">溶けません。人間じゃないみたいです。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">皆さん。もう少しだけ頑張って下さい。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Lyla">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">家族と隣人がいると心強いです。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">母が大層お怒りにならないよう、私たちが解決するんです。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">早く掃除していきましょう。今度の夜の掃除準備もする必要があります。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">この本がL社で掃除をする隣人にとって大きな助けになるでしょう。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">みんなが無事に仕事を終えられたら…いいですね…。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">掃除するときは周りに気を遣いません。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">残った隣人と家族は気にしなくても大丈夫です。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">状況に文句を言うときではありません。掃除は続けなければなりません。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">溶かす前に本になりましたね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">大丈夫です。まだ傷が深くはありません。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">まだ片付けるべきものが山積みです。</BattleDialog>
    </Type>
  </Character>
  <Character ID="sweepers">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">全ては母と家族のためです……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">裏路地よりは掃除するものが多そうにありません。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">私たちは掃除をしなければならないだけです。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">家族の力は…悪いものではありません…。良いものです…。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">あの本を持って帰れば良いんですか？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">帰りましょう。夜の時間の準備しないといけません。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">ごめんなさい…隣人の皆さん…。</BattleDialog>
      <BattleDialog ID="DEATH_1">掃除を終わらせることができませんでした…。</BattleDialog>
      <BattleDialog ID="DEATH_2">待っている家族がいます……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">まだ掃除できます。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">大丈夫です。燃料がまだ残っています。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">ずっと前に進んでいかないといけません。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">溶かす前に本になりましたね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">大丈夫です。まだ傷が深くはありません。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">まだ片付けるべきものが山積みです。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>