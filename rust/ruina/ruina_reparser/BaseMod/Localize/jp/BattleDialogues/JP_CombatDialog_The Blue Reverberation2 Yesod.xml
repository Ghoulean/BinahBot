<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Yesod</GroupName>
  <Character ID="Y_E_Eileen">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">私たちはあの方の指揮に合わせて動きます。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">あの方のために、歯車たちは回る準備を終えました。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">やはりあの方に従うことが正しい選択でしたね。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">歯車たちが噛み合って回る音が美しく響いていますね。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">私たちはいつでもまた回っていくことができます。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">美しい歯車の犠牲です。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">歯車が1つズレてしまいましたね。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">残念ながらここが目的地の歯車でしたね。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">今まで手伝っていただいてありがとうございます。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">心配なさらないでください。痛みは一瞬です。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">歯車の言葉が正しかったのです……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">今からでもあなたの歯車を見つけて差し上げます。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">歯車に……感謝を……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">定められた道へと回って行くべきでしたね……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">自分がどんな歯車なのかも知らずに……虚しく……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Y_E_Church of Gears">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">今から私たちがどうやって回るべきか教えて下さい……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">今この瞬間が……我々が転がるべきときなのですか？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">どうか私の進むべき道を……！</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">歯車が回るように……人生が廻る！！</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">歯車の導くまま……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">これ以上悩むことはない……歯車は正しかった……！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">幸せを求めて、ゆっくりと歯車は回るだけ……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">道に迷った私たちをここまで導いていただきありがとうございます……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">やはりこの道が正しかっ…た……。歯車が正しかった……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">感謝……そしてまた感謝を……！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">どうしてこんな道へと……導いたのですか……。</BattleDialog>
      <BattleDialog ID="DEATH_1">おっしゃる通り動いたのに…どうして……。</BattleDialog>
      <BattleDialog ID="DEATH_2">私は…もう要らない歯車なんですか？</BattleDialog>
      <BattleDialog ID="DEATH_3">まだ動けます……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">歯車1つが……止まってしまいました……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">要らない歯車だったんですか……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">これもまた……歯車の意でしょう……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">決められた道を歩いてきたのに……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">正しい道を教えてくださりありがとうございます……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">やはり賢明な選択です……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">最初から歯車に従えば……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">歯車の保護のもと、ここまで来ました……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">歯車にまだ会えずに居るあなたが可哀想ですね……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Y_E_Yesod">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">はい。それでは私は今から責任を果たします。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……みなさんお疲れさまでした。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">もう少し注意を払うべきでしたね……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">今までお疲れさまでした。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……状況がそれほど良さそうに思えませんね。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">まだ止まれません……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">この死を無駄にすることは出来ません。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">粛々と受け止めるべきでしょう。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">まだ戦闘は終わっていません。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">あともう少しです……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">もう、死の恐怖に慣れてしまいました。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">油断できません。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">私はやるべきことをやっただけです。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Y_E_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">イェソドさん、準備終わりましたか？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">勝てたからって嬉しいってわけではないですね。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">それでも何とか最後まで耐えました。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">今更止まれません。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">どこまで耐えられるんだろう……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">今は戦闘に集中します。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">……先に行くんですね。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">……あなたもお疲れさまです。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">ゆっくり目を閉じてください……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">お互いに最善を尽くしたんです。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">ごめんなさい。ただ申し訳ない感情で一杯です。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Y_E_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">正体は分からないけど、やってみるしか……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">見てよ！やりきったんだよ！！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">今度は上手くやれると思ったのに……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">耐えないと。私なら出来るよ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">許せない。もう我慢できない。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">ちっ、そんな単純な問題じゃないんだって！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">うるさいのがやっと止まったな……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">恨まないでよ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">歯車が導いてくれるのか……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">ベストを尽くすだけだ。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Y_E_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">頭の後ろで回ってるあれってなんです？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">なんとか終わったみたいですね……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">それでも最後まで戦いました……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">涙を流してる暇はないです。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">戦わないと！まだ終わったわけではないから……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">私たちだけでも耐えないと。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">私のこと舐め腐りすぎてません？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">もう恐れたりはしませんから。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">あなたに恨みはありませんでした……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">どうかゆっくり休んでください……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Y_E_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">なんだろう……あれがねじれなのか？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">なかなかややこしい戦いだったよ……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">こんなの認めない……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">しっかり前を見て！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">怖くないと言えば嘘かな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">君の分までベストを尽くすよ……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">運が悪かったんだろうね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">予想してなかったわけじゃないから……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">きれいに片付いたな。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">もう少しだけ集中しようか。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>