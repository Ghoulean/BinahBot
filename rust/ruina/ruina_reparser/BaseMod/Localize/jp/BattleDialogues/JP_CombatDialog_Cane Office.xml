<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Cane Office</GroupName>
  <Character ID="Nemo">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">は！は！は！楽な相手ではないようですね。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">これこれ、よく会う間柄になるはずですからよろしくお願いします！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">心配ご無用です！勝てない理由はないですからね。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">思ったより見すぼらしい方が多いようです。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">タダで情報を得られるだなんて！これから沢山お会いしましょうねぇ！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">一体あのイカレ野郎の計画どんななら……こんな場所に……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">大丈夫です！私はまだピンピンしてます。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">おや、怪我しないように気をつけないと駄目じゃないですかぁ！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">自分で気を付けないといけないのです！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">遅すぎて目で追いかけられますねぇ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">特異点の秘密を手に入れるのは……時間の問題ですねぇ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">簡単ですね！皆さんもう少しだけ力を出しましょう！</BattleDialog>
    </Type>
  </Character>
  <Character ID="Martina">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">社長……興奮しすぎないで……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">……契約内容さえ詳しく調べれば。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">いつもの仕事だから……慌てさえしなければ……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">W社の情報が……この中に……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">これでひと安心……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">こう……死ぬんだ……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">お疲れ……代わりに防げなくてごめん……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">死ねば本になるってことか……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">段々押されてるみたい……状況をちゃんと把握しないと……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">目的のためなら……やらないといけないから……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">残り少ないみたい……もう少し頑張ろう……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">これを片付けても、溜まった仕事がいっぱいあるのに……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Bada">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">社長のおっしゃる通り上手く解決できるでしょう。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">お願いですから興奮しないで、落ち着いて前を見てください。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">本さえ手に入れて帰ればいい。困ることはないでしょう。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">皆さん無事ですか？ 状態を確認することも重要です。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">本は私が取っておきました。帰りましょう。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">帰らないと……いけないのに。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……黙祷を捧げます。これからはゆっくり休めることを。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">わたくしの力量が足りないせいです。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">もうひと方でも生きていらっしゃるのが大切なのに……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">つまらないやつら……難しくありません。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">手間はそう掛からないですね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">ほとんど片付いていってる模様です。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>