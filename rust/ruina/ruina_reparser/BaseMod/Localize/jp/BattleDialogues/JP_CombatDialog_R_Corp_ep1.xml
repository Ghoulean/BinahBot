<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>r_corp_ep1</GroupName>
  <Character ID="R_Corp_Rudolph_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">馴鹿たち、早くボコして出ていこうか。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">弱そうな相手ではないみたいだ。気を引き締めていこう。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">他のチームの助けなしでも何とかなったな。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">建前上、お疲れとは言っておく。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…まだだ…まだ終わってないのに……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">もっと注意していかないとな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">危なくなってきた…精神が完全じゃない。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">全ては精神力の戦いだ……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">僕らを防ぐのにはちょっと足りなかったみたいだな？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">馴鹿たち。残りは少ない。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">急げ！長引かせたら不利になるのは僕たちだ。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Reindeer_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">今日も出撃か。みんな頑張ばろうか。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">なんだよ急に……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">どうしようもないだろ。みんな処分されたいのか？</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">まぁ、今までやってきたことも生きるためだったからな。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">一気に終わらせようか。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">もう少しでも時間を稼がれたら危なかった。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">れで殺処分は免除されるのか？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">知らないよ。上で良い感じに処理してくれるだろ。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">ルドルフさん、帰りますか？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">相手を舐めすぎたか……。</BattleDialog>
      <BattleDialog ID="DEATH_1">他のチームが勝つことを祈るか……。</BattleDialog>
      <BattleDialog ID="DEATH_2">情報が足りなかった…もっと調べて来れば……。</BattleDialog>
      <BattleDialog ID="DEATH_3">ここも怪物ばっかりだな……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">耐えなければ…みんな耐えよう。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">これは…予想外の状況だ……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">焦ってはいけないと分かっているのに……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">こんな力は……深層でしか見たことがないのに。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">良い武器もまともに使えなければ意味がないんだ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">今のところ問題はないから戦い続けるか。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">…耐えづらくなってる。急いで終わらせないと……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">他のやつらも段々疲れていってるみたいだな。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Maxim_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">ここで死ぬか生きるかは自分の選択にかかっている！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">今は暴れることが俺たちの任務だ！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">細かいことはあいつらを倒した後だ！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">遺跡に居るやつらよりも手応えあったな！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">思ったより大したことないな、だろ！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">全然暴れまわれないまま……こんな風に負けるとか……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">あと何匹だ？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">結構厄介な相手だな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">おっとぉ！まともに踏み潰されたか。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">力で押しつぶせばテクニックも何も無駄だ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">突進すれば勝手に道が開かれるからな！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">サイの角のように独りで征け！</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Rhinoceros_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">迷わず果敢に！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">どんな敵であれ、サイの足に押しつぶされるだけだ！ </BattleDialog>
      <BattleDialog ID="START_BATTLE_2">誰から潰そうか！</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">サイの前を塞げるものは何もない！</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">隊長。今すぐ始めれば良いんですか？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">勝利だ！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">勝てると思ったか？惜しいな！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">これで殺処分されるのは保留だな。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">久々の本気で戦える相手だったな！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">これくらいで折れたりはしない……！</BattleDialog>
      <BattleDialog ID="DEATH_1">…かはっ…耐えなければ……！</BattleDialog>
      <BattleDialog ID="DEATH_2">このまま倒れたりは……！</BattleDialog>
      <BattleDialog ID="DEATH_3">クソッ…こっちがやられたか……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">もうちょっとズレてたらこっちが殺られてたな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">いつものことだ！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">群れの全滅に繋がらないと良いな！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">気にしない！やるべきことがあるから！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">身体が凄くか弱いな、だろ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">そんな攻撃は無駄だ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">まだ全部終わったわけではない！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">前に立ちはだかるとは……愚かだな！ははっ！</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Myo_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">ウサギが草を食みに来たよ！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">ウサギに任せて！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">草があればどこだって大歓迎だよ！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">食べ応えのある食事だったね。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">ふぅ～疲れた。じゃあ帰ろうか？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">かはっ……はは……油断しちゃったね……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">1匹のウサギが草原に還った。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">今も昔も、ここは相変わらず危ない場所だなぁ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">むしろ外郭の任務のほうがもっと面白かったかもね。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">まだ食める草がたっくさん残ってるね！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">死ね、ははっ！大したことないね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">あの向こうの怪物を始末するのがもっとややこしそうだね！</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Myo_Meet_Gebura_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">まだあの機械の操り人形なんかやってるの？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">昔のあたしは知らなかっただろうな。あたしの英雄がこうなるだなんて。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">うん。過去はただの過去。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……悪くないな。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">ウサギたち、耳をピンと立てて集中するように。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……いつも通りに。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">1匹のウサギが草原に還った。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">あの赤い霧の下にいるやつらが…こんな弱っちぃのかぁ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">あたしも遊んでたわけじゃないから。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">ウサギがどれだけ強いのかちゃんと見せてやろうか。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">覚えてる？生き残ったやつが強いって言葉。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">今は状況が違うってことを頭に入れておかないとね。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Gebura_Meet_Myo_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">相変わらず口数が多いな。言いたいことはそれで終わりか？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">もうお前が私のことをどう考えようが知ったこっちゃない。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……二度と会うことがないよう祈ってる。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">クソッ…でも結果は受け入れないとな。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">全員信じろ。あのウサギたちを皆殺しに出来る。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">しっかりしろ。まだ終わってないから。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">あいつの犠牲を無駄にはしたくないだろ？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">勝てない理由は無いよな。全員落ち着け。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT" />
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">多くが変わったけど、お前の言うその心はより強くなったから。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">お前の粗末な動きは相も変わらないな。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Rabbit_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">１つ残らずかじってしまおうか！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">草原を荒らし回る時間だ。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">美味しい食事を楽しむ準備は出来た？</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">ありゃぁ！草が足りなくはないよね？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">勝利を決定するのは火力の問題だ！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">はは！こんな戦闘は久々だな。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">これ以上食む草は無いのか？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">あはは…このままじゃ本当に死にそうだなぁ……。</BattleDialog>
      <BattleDialog ID="DEATH_1">本当にウサギだけで解決できるのか？</BattleDialog>
      <BattleDialog ID="DEATH_2">みんな死ぬんだ……。</BattleDialog>
      <BattleDialog ID="DEATH_3">こんな任務自体、話にならなかったんだよ……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">運がこれっぽっちもないやつら。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">ここも頭おかしいところだな！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">そっちのこと過小評価しちゃったか？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">これちょっと危ない感じだな？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">都市の星とはいえ、ウサギには草に過ぎない。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">食める草が段々減ってく。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">かなり良い感じの席だったな？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">まだまだ余裕！もっと早く動いてみるか？</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Nicolai_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">うるさい。全員黙って集中しろ。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">ウサギたち。生きるために来たから、生きようと努力しないとな？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">くたばりたくないなら真面目に臨むように。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">帰って報告すべきことが山積みだな……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">また会うときは…こんな風に殺られたりはしない。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">周りを気遣う余裕ないのは分かってるよな？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……ウサギたち、しっかりするように。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">段々と機が熟している…もう少しだけ耐えるように。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">警戒態勢を解くな！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">大したことないやつらだった。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">一筋縄ではいかなかったが、それでも可愛いらしい実力だな。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">残り僅かだ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">慌てる姿がとても良く見えてくるよ。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Nicolai_Meet_Gebura_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……ミョ、知り合いか？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH" />
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT" />
  </Character>
</BattleDialogRoot>