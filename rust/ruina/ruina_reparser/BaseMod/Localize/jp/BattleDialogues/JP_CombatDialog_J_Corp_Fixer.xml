<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>JCorpFixer</GroupName>
  <Character ID="hanahuda">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">一点張りしないでください。そのうちひどい目に遭いますよ。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">準備は終わりましたか？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">かなり興味深そうな相手ですね。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">つまらないなぁ。でも……報酬は悪くないですね。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">私が追いやられるだなんて……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">完全にお流れになったか……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">落張不入。もう諦めることも出来ません……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">まぁ、盤面がこうなら私にとっては良いけど。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">このまま追い詰めましょうか。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">そろそろ本格的に手合わせといきますかね。</BattleDialog>
    </Type>
  </Character>

  <Character ID="JCorpFixer">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">勝っても終い、負けても終いなら…どう人生を楽しむんですか？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">気をつけて準備して腕前を発揮するのは…気に食いませんね！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">行きましょうよぅ～！戸惑ってたら盤面終わっちゃいますよ！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">この勝負は…私たちが勝ったんですよね？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">今回の盤面は成功でしたね！ですよね、ハナフダさん？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">凄い報酬ですね、良いですね！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">一発食らいました……。</BattleDialog>
      <BattleDialog ID="DEATH_1">彼らがどんな気分で足掻いたのか……。</BattleDialog>
      <BattleDialog ID="DEATH_2">正当な勝負の結果……受け入れます。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……いや。最後に勝つのは絶対に……私だ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">大切な仲間を失うって言ってたけど……やっぱり。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">まだ対局は終わってない。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">五光で～す！防いでみてくださいよ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">ふふ……このままなら簡単に勝てそうですね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">簡単すぎませんか？ため息しか出てこないね！</BattleDialog>
    </Type>
  </Character>
  
  
</BattleDialogRoot>