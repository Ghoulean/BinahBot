  <GroupName>Normal Enemies</GroupName>
  <Character ID="Stray_Dogs">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">どっからこんなザコどもばっか集めたんだ？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">アニキの命令だし、しゃあねぇな。やるしか。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">どれ、ちょっと身体ほぐしてみっか。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">つべこべ言わず掛かってこいよ。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">アニキの言ってた本を探す。いいな？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">こんな紙クズのために大騒ぎしたってか？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">早く帰って休みてぇな。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">こんな簡単だとか。呆れたぜ……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">はぁ、その足で立ってられるのが不思議だよ。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">楽しくもねぇし、二度とやりたくねぇな。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">はっ……！気分クッソ最低だな。</BattleDialog>
      <BattleDialog ID="DEATH_1">クソッ……こんな風に死にたくはなかったんだがなぁ。</BattleDialog>
      <BattleDialog ID="DEATH_2">この……生意気な……！</BattleDialog>
      <BattleDialog ID="DEATH_3">ああやって攻撃するとか反則だろ！畜生……チクショウ！</BattleDialog>
      <BattleDialog ID="DEATH_4">俺が一番の馬鹿だったか……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">お前が死んだらアニキにまた説教食らうんだって！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">全員気を付けろっつってんだろ！畜生、余計面倒じゃねぇか。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……そうだな、俺さえ生き残ればそれで良いんだ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">別に、あんな弱っちいヤツ１人くらい失せても勝てる……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">これでサヨナラとか、ちょっと寂しいなぁ。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">ザコかよ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">もうぶっ倒れたのか、つまんねぇな。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">そうやってしゃしゃり出て来っからぶっ倒されんだよ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">もうおねむの時間ですかぁ？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">あははは！お前らも見るんだったな、さっきくたばったやつの顔！</BattleDialog>
    </Type>
    <Type ID="ENCOUNTERS_OPPONENT" />
  </Character>
  <Character ID="lulu_friend1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">まっかせて～！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">あいつらそんな強いって？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">ハンマー１発食らっとく？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">よ～いしょっと！疲れたけど結構やりがいはあったよ！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">よし。こんくらいのやつらなんて朝飯前だって！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">あれ、拾っとけば良いんでしょ？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">え……死んじゃうの……？</BattleDialog>
      <BattleDialog ID="DEATH_1">何これ……ほんと情けないな……。</BattleDialog>
      <BattleDialog ID="DEATH_2">ムカつく……このまま死ねるか！</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">もっと頑張ら……ないと。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">どうすれば……この状況を乗り越えられるんだろう……？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">あんな風になりたくない……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">よし、他の奴らも片付けちゃおう！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">やっほぃ！さっきのどうだった？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">ホント、片付けやすいやつだねぇ！</BattleDialog>
    </Type>
    <Type ID="ENCOUNTERS_OPPONENT" />
  </Character>
  <Character ID="lulu_friend2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">つまんなさそうなやつばっかだね？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">串刺しにしてやる！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">力になれるなら！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">他の本も持って帰れたりする？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">よゆ～よゆ～。こんくらい楽勝！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">ちょっとギリギリだったね……ふぅ！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">ごめん、最後まで手伝えなくて……。</BattleDialog>
      <BattleDialog ID="DEATH_1">どうして……こんな場所に来たんだろう？</BattleDialog>
      <BattleDialog ID="DEATH_2">大丈夫だと思ったのに……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">少し……怖くなってきた。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">気、気にしない……よ、うん……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……みんな一緒に勝てると思ったのに。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">うぅ、クソ野郎が。思ってたより強いじゃん！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">つまんないやつ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">結構いい感じじゃ？もっと来な！</BattleDialog>
    </Type>
    <Type ID="ENCOUNTERS_OPPONENT" />
  </Character>
  <Character ID="Yun_Fixer2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">先に攻撃できた方が多分有利だよね？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">ここに入れるくらいの根性くらいはあるんだってばぁ～。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">猫探しよりは楽しそうだよね？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">もっと強いやつが来れば1分も耐えられなかっただろうね？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">そんな、私たちにも勝てなかったの？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">こいつらも、まだまだだよねぇ～。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……行くなって言ったの。聞いておけばよかったかな……。</BattleDialog>
      <BattleDialog ID="DEATH_1">もっと高いところに行きたかったのに。</BattleDialog>
      <BattleDialog ID="DEATH_2">悔しい……悔しくて耐えられない。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">うそ……でしょ……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">こんな怖い場所だったとか……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">今でも逃げたほうがいいんじゃない？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">ふん、弱っちいねぇ！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">隙はっけ～ん、失礼！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">まぁ……偶然だったけどね！</BattleDialog>
    </Type>
    <Type ID="ENCOUNTERS_OPPONENT" />
  </Character>
  <Character ID="Yun_Fixer1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">うぅ……大口叩いたけど、やっぱちょっとアレだと思うんだけど……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">いやぁ、図書館ってこんなに大きいのか……？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">思ってたより強そうだけど……勝てるか？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">簡単な仕事もこなせないって思うだなんて、みんな心配性だな。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">ビビって損した……こんな簡単に終わるとか！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">これだけ本があれば……たんまり報酬が貰えそうだなぁ！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……俺が……どう……どうしてこんな目に。</BattleDialog>
      <BattleDialog ID="DEATH_1">結局、俺は上に登れなかったのか。</BattleDialog>
      <BattleDialog ID="DEATH_2">もう目を開ける力も、意地もない。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">段々危なくなってる気が……するんだが……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……今の見たか？本……が……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">こんな簡単に終わるとか……信じられねぇ。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">そんなんでお宝とやらが守れるのか？あはは！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">俺の相手にもならなかったじゃねぇか！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">こっちも遊びでこんな事やってるわけじゃないんだ。</BattleDialog>
    </Type>
    <Type ID="ENCOUNTERS_OPPONENT" />
  </Character>
  <Character ID="Yun_Office">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">代表ってホントに来るの……？あーもう……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">時間だけ稼ごう……。まともに相手するには怖いし。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">いつまで私たちが相手しなきゃいけないの……？</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">先陣切るのって私苦手なんですよぉ……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">でもすぐ行くって言ってたし……頑張って耐えよう……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">何だよ、代表が来る前に終わっちゃったじゃん。楽勝！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">持ってくもん持って帰れば良いんでしょ？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">ここで本になったとか、ホント情けない人ばっかだなぁ！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">ま、まさかこの前はもっと強いやつだったとかないよね……？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">これで帰っていいの？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……今度はうまく……行くと思ったのに。</BattleDialog>
      <BattleDialog ID="DEATH_1">死にたくない……まだ生きたいのに……。</BattleDialog>
      <BattleDialog ID="DEATH_2">やられたなんて……情けない……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">ためらってるわけにはいかないし……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">身体が弱いなら、せめて目が早くなきゃ……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">大事な仲間が……どうしたらいいの……？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">恨みはない。ごめんなさい。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">……よし、次はどうやって片付けようか？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">上手く行けばこっちが勝ちそうじゃない……？</BattleDialog>
    </Type>
    <Type ID="ENCOUNTERS_OPPONENT" />
  </Character>
  <Character ID="Zwei_Members">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">「あなたの盾」ツヴァイ。ただいまより開始いたします。</BattleDialog>
