<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Hokma</GroupName>
  <Character ID="H_P_Pluto">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">準備なさったのなら……始めます。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">誰かが見せてくれる世界に騙されない方が良いですよ。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">どうか、お互いに恨みはありませんことを。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">また……騙されたか。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">目に見えるものをそのまま信じてはなりません。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">ご容赦ください。悪意はありませんでした。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">わたくしがやるべきことをやるだけです。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">だんだんと近づいているようですね。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Hokma">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">とはいえ、止められないことはないでしょう。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……最後まで信じるしかないでしょうね。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">あとは残った者たちに任せても大丈夫です。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">自責せずにゆっくり目を閉じてください。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">動揺することは良いことではありません。息を落ち着けてください。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">我々は皆、既に止まれない道へと歩みを進めているのでしょう。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">そうですね、ホクマーさん……何か方法はありますか？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">悔しくないのは……ただの強がりだ……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">他の皆さん無事ですか？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">心配しないでください。自分のことは良く分かってるので。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">立ち止まってしまえばそこで終わりですから……。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">無事に乗り越えられてよかったです。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">まともな方法で戦えそうにはないなぁ……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">お役に立てず済みません……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">自分を信じて……進もう。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">自分の状態を1回くらいは確認しないとな。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">状況は……いつでも変わり得るって信じてる。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……本当に終わったんだよね？</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">勝てるって信じないと……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">こんな手を……使うだなんて。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">そろそろ終わりだ……もう少しだけ……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">そんな闇雲に前に出ちゃ……！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">心配だなぁ……本当になんとかなるって思ってる？</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">危なかった……止められなかったらどうなってたことか……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">急いては事を仕損じますよね……準備します。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">このままじゃ……危ないのに……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">もう少し集中した方がいいと思います。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">嘘を……見せる、か。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">大きく出られます…皆さん気をつけてください。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">どこまでが信じるべき真実なのか分かりませんね。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>