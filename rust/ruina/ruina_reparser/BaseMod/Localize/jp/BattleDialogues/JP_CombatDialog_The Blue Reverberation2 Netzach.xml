<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Netzach</GroupName>
  <Character ID="N_B_Bremen">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">キャﾝｷｬン！ コｺコ～ッ、ヒｨイ～ン！！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">ｺッ……ｺコ～ｯ！！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">キｯホ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">ｺｯ……ｺッ…。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">キャン！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">キャﾝｷャンキャﾝ！！キャﾝキャンｷャン！！！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">ｺ～ｯ！！ｷｬﾝキャン！！！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">ヒｨイ～ン！！！</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Netzach">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">うん……やってみなきゃ分からないから。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">そこまで……気には食わないなぁ。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……今回も役に立てなかったな。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">状況が悪くなってるみたいだ……みんなもう少しだけ気を引き締めよう。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">みんな生きたくて必死に戦ってるんだろうな……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">もう逃げたりしないから……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">あまり……良い状況ではなさそうだな。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">もうこんなことにも慣れていってるな……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">はぁ……まともじゃないよ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">うるさい音がだんだんと消えてるみたいだな……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">もう少しだけ頑張ろうか……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">まぁ、私たちは止められないわけもないですよね。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">ネツァクさん……他の階は大丈夫ですかね？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">良くない。このままじゃ……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">これで本当に大丈夫かな……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">まだ止められる可能性は十分にあります。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……段々と押されてるだろ。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">先に片付けるべきですよね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">この流れなら……無理なこともないですね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">どうすれば良いか……大体分かりました。</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">はい、ネツァクさん。本当に大丈夫なんですかね？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">無事に終わってよかったです。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">私が居なくても……大丈夫ですかね……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">危なさそうだけど……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">撤退は……できませんよね。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">みんな……凄く疲れてますね。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">よし……まともに当たった。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">横にいるやつもまともな姿をしてないな……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">こんなもん誰だって早く終わらせたいですよね……。</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">はっ、頭が3つもついてる……。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">ややこしかったな……舐めて掛かれなかったよ。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">マジで化け物みたいなやつじゃんか……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">あれ何だよホント…どうやってあんな力を？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">頭痛いなぁ…どうすれば良いんだ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">どうにか……解決できるはずだよな。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">危なかったぁ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">もう少しだけ耐えようか。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">よし。ちょっとは活路が見えてきたな？</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">ねじれか……雰囲気やばいなぁ。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">もう少し長引けば危なかっただろうな……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">すみません……これ以上は耐えられ……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">生き残ったやつは気を引き締めろ。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">くそっ……全然強いじゃないか。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">どうすれば力を使わせずに済むか……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">面倒だな……あとどれくらいだ？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">全部片付けられそうか……？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">勝算が全く無いわけではなさそうだな……。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>