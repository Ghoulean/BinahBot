<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>HanaAssociation</GroupName>
  <Character ID="Hana_Fixer_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">人生長く生きてるとこんな変な場所に来たりもするんだな。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">報告通りだな。図書館……みたいな感じはしないけど。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">どこかでよく見た服装もあるなぁ……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">あんなふうにここに来たゲストの力を奪うのか……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">とにかく……これからよく顔を合わせることになると思うので、よろしくお願いします。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">やっぱりややこしい相手だったな……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">本が図書館の分析に役立つと良いんだけど。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">帰って報告すべきことが多いなぁ。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">部長は無事だろうか……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">状況は整理出来たみたいだし……帰るか。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">気が緩みすぎた……。</BattleDialog>
      <BattleDialog ID="DEATH_1">どうしようもないのか……。</BattleDialog>
      <BattleDialog ID="DEATH_2">やはり……我々の力だけでは……。</BattleDialog>
      <BattleDialog ID="DEATH_3">少なくとも足枷程度には……。</BattleDialog>
      <BattleDialog ID="DEATH_4">残念だけど……諦めるときが来たんだろう。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……力の限りやらないと。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">まだ大丈夫なはず……きっと。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……他の方は大丈夫でしょうか？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">そろそろ終わるはずだから……皆もう少しだけ耐えて。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">致命的じゃあない。少なくとも今は……。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">隙が見えます。焦ってるようですね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">あともう少しだ。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">こっちもそう余裕ぶっていられないな。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">逃しません！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">このまま周囲を警戒するように。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hana_Fixer_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">ミリネさん、始めてよろしいでしょうか？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">心配なさらずとも。無事に片付けます。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">はい、横で補助にあたります。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">とにかく……これからよく顔を合わせることになると思うので、よろしくお願いします。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">いつも通り進行します。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">やはり一筋縄では行きませんでしたね……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">それでも大事もなく処理できた気がしますね。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">全員無事ですか？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">こんな依頼は毎回震えが止まりませんね……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">私は大丈夫だと思います…皆さん怪我は無いですか？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">悔しくは…ありません。</BattleDialog>
      <BattleDialog ID="DEATH_1">ごめんなさい…これ以上は……。</BattleDialog>
      <BattleDialog ID="DEATH_2">やはり……我々の力だけでは……。</BattleDialog>
      <BattleDialog ID="DEATH_3">気が緩みすぎた…もっと気をつけるべきで……。</BattleDialog>
      <BattleDialog ID="DEATH_4">油断しました……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">致命的じゃないです。少なくとも今は……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……力の限りやりませんと。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">まだ大丈夫なはずです。きっと。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">……皆さん特に異常は無いですか？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">どうせ逃げられないのであれば、最後まで立ち止まりません。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">隙が見えます。焦ってるようですね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">あともう少しですね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">こちらも余裕はありません。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">逃しはしません！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">このまま周囲を警戒してください。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hana_Mirinae">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">集中して。まだ実力が分かってないから。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">結果を見せられるようにしようか。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">それぞれの実力を十分に発揮できるように祈ってる。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">こう簡単に負けて欲しくなかったのに。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">望むものがあれば、失うことも覚悟しないと……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">平常心を維持するように。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">駄目なら私たちの運命はここまでってこと。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">私たちも一切の対策もなしに来たわけじゃないから。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">予測する必要もない。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">最後まで緊張の糸を切るな。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">お前らこそ、私たちを過小評価しすぎたな。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hana_Harold">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">距離が近い……とりあえず、相手の行動を見つめよう。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">ここだからって他の星とそう変わらないだろう……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">ミリネ部長。準備完了しましたか。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">そこまで難しいこともなかった……。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">既に空高く登った星だってことか……。</BattleDialog>
      <BattleDialog ID="DEATH_1">全てを飲み込む力が…思ったより……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">引き下がることも出来ないな……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">残りの人員たちだけでもしっかりしようか。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……ああ。このままじゃ殺られるな。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">このまま上手くいくと良いけど……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">……そろそろ終わりが見えて来た。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">個々人が無視できないほど強いな……。</BattleDialog>
    </Type>
    <Type ID="ERROR" />
  </Character>
</BattleDialogRoot>