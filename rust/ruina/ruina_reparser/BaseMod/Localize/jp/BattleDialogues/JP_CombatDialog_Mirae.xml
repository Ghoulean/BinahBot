<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Mirae</GroupName>
  <Character ID="Named">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">事が複雑にこんがらがりましたね。そろそろ解いてみますかね？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">最近はほんっと変なことが多くて……煩わしいことになりました。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">どれどれ……問題はなさそうですかね？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">情報は十分です。戻りましょうか？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">ああ……惜しくも条件から外れてしまいましたね。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">こんな風に終わるのか……。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">厄介な事件と絡んでいるな……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">まだ大丈夫です。みんな集中してください。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">自分の身体に気をつけないと。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">ごめんなさ～い。まあ、私たちのやることっていつもこういうのじゃないですか。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">他の情報も分かっていくし……悪くないですね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">ははは。年は食ったけど、引退には早いので……。</BattleDialog>
    </Type>
  </Character>

  <Character ID="Mirae">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">面倒なことになりました……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">いつも同じことばかり担当してますからね……。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">特に問題ないですよね？</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">イリーナさん。気をつけたほうがいいと思います。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">そうですね…最近都市の星へランクが上がったそうです。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">全部記録しました。帰りましょう。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">本当に変な場所でした…特異点だか何だかは分からないけど。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">最近にはわけの分からないことがよく飛び込んできますね……。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">それでも1件はちゃんとこなせて良かったです。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">一体…何のために入る場所なんだ。ここは……。</BattleDialog>
      <BattleDialog ID="DEATH_1">いつも覚悟できてるって…思ってたのに……。</BattleDialog>
      <BattleDialog ID="DEATH_2">だから今回の依頼は危ないって……。</BattleDialog>
      <BattleDialog ID="DEATH_3">もういいです…自分の身を守ってください……。</BattleDialog>
      <BattleDialog ID="DEATH_4">もうこんなことは……。이제 이런 일은…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……やはり、支援を受けるべきでした。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">多分、依頼者さんの言うことが嘘ではなかったみたいですね。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……結構大変なことになってますよね？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">戻って整備し直す必要があると思います……。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">私たちだけで解決するには難しいと思います。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">残りも早く片付けましょう。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">残り少ないようです。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">舐めて掛かれるわけではないですね。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">記録すべきことが多いですね……。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">少しでも乱れては駄目です。</BattleDialog>
    </Type>
  </Character>
  
  
</BattleDialogRoot>