<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Hod</GroupName>
  <Character ID="H_G_Greta">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Hawhawhaw! Is it prep time for the ingredients?</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">That’s a selection of fascinating fixings there!</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">Wonder how the others are faring!</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">Well… I’ve met a real delicacy here…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">Aw, that sucks… So I can’t cook ‘em on the spot?</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">Chump! I could eat you up in one bite.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">Not the most refreshing flavor! Being honest.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">This is giving me a big batch of fresh inspiration!</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Hod">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">I’ve learned a lot of things watching you all.</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…As long as I could be of some help.</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">I’m still okay… How about you?</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">…We’ve got to find a good way out of this.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">Y-yeah, I’m okay. I won’t falter!</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">I didn’t mean to crush your dreams. I just…</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Miss Hod. Are you going to be alright?</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…I had it coming.</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">No… …We’ll be at a bigger disadvantage if we aren’t careful.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">She hasn’t put us in the corner just yet.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">We should read her movement…</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">…It seems we managed to overcome this adversary.</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">We have ourselves to worry about.</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">I couldn’t help much… yet…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">We have to keep cool… and move on.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">Such violence…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">This isn’t the worst of the worst… I think…?</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">We could handle this, thankfully…</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">I’m worried for the other floors… Will they all be fine…?</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">I think I… did as much as I could…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">Sorry. There’s nothing I can do for you now…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">If I don’t watch out, then soon… I might…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">The tide hasn’t turned yet…</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">We pulled ourselves up to put a stop to her…</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Just how many mouths does she have…</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">Shoot… I was careless…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">We should be more careful and get out of her way…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">That shark gal… Isn’t she being too reckless?</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">Does she really see us as food for huntings…?</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">Sigh… That bulky weapon was kinda menacing.</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>