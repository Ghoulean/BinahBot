  <GroupName>r_corp_ep2</GroupName>
  <Character ID="R_Corp_Rudolph_2_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">순록들, 빠르게 치고 나가자고.</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">이미 한 번 겪었잖아?</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">전투 방식이 뻔하군. 단숨에 이겼어.</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">이번에도 밀리는 건 위험한데…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">슬슬 한계야… 빨리 끝내야 하는데…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">모든 건 정신력의 싸움이지…</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">전과 같을 거라고 방심했나 보네.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">순록들. 얼마 남지 않았어.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">서둘러! 길게 끌면 불리해지는 건 우리니까.</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Reindeer_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">하아… 다시 한 번 출격이네.</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">이번에는 확실히 찍어누를 수 있겠지…</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">불안해도 소용없어. 어쩔 수 없는 일이니까.</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">뭐, 살기 위해 힘내야지.</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">단숨에 마무리 짓자고.</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">조금만 더 시간을 끌었어도 위험했어.</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">이걸로 살처분은 면하는 거야?</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">몰라. 위에서 알아서 잘 처리해 주겠지.</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">루돌프님, 이제 돌아갑니까?</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">이번에는 이길 수 있을 거로 생각했는데…</BattleDialog>
      <BattleDialog ID="DEATH_1">남은 녀석들이라도… 잘 하길…</BattleDialog>
      <BattleDialog ID="DEATH_2">이제 정말 끝이라고…?</BattleDialog>
      <BattleDialog ID="DEATH_3">…이 괴물들을 상대로 이길 수 있을 리가…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">버텨야 한다… 다들 버텨야 해.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">전에도 상황이 이렇게 흘러갔던 거 같은데…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">초조해 하면 안 되는 걸 알고 있음에도…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">대체… 이런 힘은 어디서 나는 거지…</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">좋은 무기도 제대로 사용하지 못하면 안 되는 거라고.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">아직은 문제없으니 계속 싸우도록 할게.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">…버티기 힘들어지고 있어. 서둘러 끝내야만…</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">다른 녀석들도 점점 지치는 것 같네.</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Maxim_2_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">이번에는 살아서 나간다!</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">지금은 날뛰는 게 우리 임무다!</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">잡생각은 저 녀석들을 쓰러뜨린 후에!</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">하하! 제대로 우리를 무시하고 방심했나 보군!</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">몇 마리 남았지?</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">다시 봐도 골치 아픈 녀석들이군.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">이런! 제대로 짓밟혔군.</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">힘으로 찍어누르면 기술이고 뭐고 소용없지!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">돌진하면 저절로 길이 열릴 테니!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">무소의 뿔처럼 혼자서 가라!</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Rhinoceros_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">망설임 없이 과감하게!</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">어떤 적이든 코뿔소들의 발에 짓눌릴 뿐이지!</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">어느 놈부터 굳히는 게 좋을까!</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">그 무엇도 코뿔소의 앞을 막지는 못하지!</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">대장. 바로 시작하면 되는 겁니까?</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">우리의 승리다!</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">이번에도 이길 수 있다 생각했나? 아쉽군!</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">이로써 살처분 당하는 건 유보하겠군.</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">재밌는 녀석들이야!</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">우리는 이 정도로 꺾이지 않는다…!</BattleDialog>
      <BattleDialog ID="DEATH_1">…커헉… 버텨야 한다…!</BattleDialog>
      <BattleDialog ID="DEATH_2">이대로 무너지지 않아…!</BattleDialog>
      <BattleDialog ID="DEATH_3">젠장… 변한 게 없군…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">조금만 어긋났다면 내가 당했겠어.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">이미 익숙한 일이지!</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">무리의 전멸로 이어지지만 않았으면 좋겠군!</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">신경 쓰지 않는다! 해야 할 일이 있으니!</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">몸이 굉장히 연약하군 그래!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">그런 공격은 소용없지!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">아직 전부 끝난 게 아니다!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">내 앞을 막다니… 어리석구나! 하하!</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Myo_2_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">토끼들! 다시 지기 싫으면 전력을 다해!</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">다시 한 번 우리를 본 기분이 어때?</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">이번에는 전부 뜯어 먹어 줄게.</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">만족스러운 식사였어.</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">후우~ 지친다, 지쳐. 이제 돌아가볼까?</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">한 마리의 토끼가 뒈졌어.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">저 미친 새끼들…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">화력을 더 쏟아부어!</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">아직도 뜯을 풀이 잔뜩 남았네!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">똑같이 당할 줄 알고? 어림없지.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">마저 뜯고 돌아가자고!</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Myo_Meet_Gebura_2_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">또 만났네. 다시 나를 만난 소감이 어때?</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">그래. 옛날은 옛날일 뿐이야.</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">토끼들, 귀를 쫑긋 세우고 집중하도록 해.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">…늘 하던 대로.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">한 마리의 토끼가 뒈졌어.</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">그 붉은안개 밑에 있는 녀석들이… 약해 빠져선.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">나도 가만히 놀고 있던 건 아니니까.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">한 번 당한 토끼가 얼마나 독한지 보여주자고.</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Gebura_Meet_Myo_2_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">…무슨 대답을 바라는 거야. 반갑다고 말해줄까?</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">다시 이기지 못할 이유도 없지. …돌아가자.</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">젠장… …하지만 결과는 인정해야겠지.</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">이미 한 번 해봤으니까. 또 성공할 수 있을 거야.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">정신 차려. 아직 끝나지 않았으니까.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">저 녀석의 희생을 헛되이 하고 싶진 않겠지?</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">승리하지 못할 이유는 없지. 다들 침착해.</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT" />
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">…어딜 가는 거지?</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Rabbit_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">남김없이 갉아먹자고!</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">다시 풀밭을 헤집어 놓을 시간이야.</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">맛있게 식사를 즐길 준비 됐어?</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">이번에는 다를 거야! 토끼들은 굶주렸거든.</BattleDialog>
