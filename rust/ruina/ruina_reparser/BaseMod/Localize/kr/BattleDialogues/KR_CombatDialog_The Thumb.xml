  <Character ID="Denis">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">준비하자… 더 윗사람이 나서는 일이 없도록…</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">서둘러서 좋을 건 없지…</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">그래도 다른 곳처럼 무례하게 굴지는 않네…</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">정당한 보수라… 나쁘지 않아…</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…더 할 수 있는 말은 없겠지.</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">죽을 때도 뒤끝은 없도록…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">우선 받은 일부터 처리하는 데 집중해야 해…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">아직은 위험하지 않아…</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">공평한 제안을 하셨으니… 너무 분해 마시길…</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">마지막 경의를 표합니다…</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">부디 최선을 다하셨길…</BattleDialog>
    </Type>
  </Character>
  <Character ID="Boris">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">일개 전령만 아니었어도 기분이 썩 괜찮았을 텐데.</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">비겁한 수를 쓰는 건 전투에 대한 예의가 아니지…</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">얻을 게 없었다면 시작하지도 않았으니까.</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">필요한 정보는 다 회수한 것 같습니다.</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">마지막까지 흐트러진 모습을… 보일 수는 없지…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">감히 제안을 받아들인 것에 대한 정당한 대가겠지.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">서로가 약속한 것을 받아가는 거야.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">조용히 묵념만 하는 것이 예의일 테지.</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">그 기세가 거칠어도 절도가 짧아야지.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">전력을 다하는 것이 상대방을 무시하지 않는 행동.</BattleDialog>
    </Type>
