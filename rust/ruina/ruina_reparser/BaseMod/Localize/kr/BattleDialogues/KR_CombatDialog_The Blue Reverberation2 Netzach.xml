<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Netzach</GroupName>
  <Character ID="N_B_Bremen">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">컹컹! 꼬꼬꼮~ 히이잉~!!</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">꼭… 꼬꼮~!!</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">끽호.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">꼮… 꼭…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">컹!</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">컹컹컹!! 컹컹컹!!!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">꼮~!! 컹컹!!!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">히이잉~!!!</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Netzach">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">그래… 해보지 않으면 모르는 일이니까.</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">그다지… 마음에 들지는 않네.</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…이번에도 도움이 되지는 못했구나.</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">위험해지는 것 같은데… 다들 조금만 더 긴장하자.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">다들 살고 싶어서 열심히 싸우는 거겠지…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">이제 도망치지 않을 테니까…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">상황이… 좋아 보이지는 않네.</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">이제는 이런 게 익숙해져 가네…</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">하아… 제정신은 아니야.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">시끄러운 소리가 점점 사라지는 것 같네…</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">조금만 더 힘내보자고…</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">뭐, 우리가 막아내지 못할 이유도 없겠죠.</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">네짜흐 님… 다른 층은 괜찮을까요?</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">좋지 않아. 이런 식이라면…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">이대로 정말 괜찮을까…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">아직 막아낼 가능성은 충분해요.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">…점점 몰리고 있잖아.</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">마저 처리해야겠죠.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">이런 흐름이라면… 불가능할 것도 없겠어요.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">어떻게 하면 좋을지… 대충 알 것 같네요.</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">네, 네짜흐 님. 저희 정말 괜찮은 걸까요?</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">무사히 끝낼 수 있어서 다행이에요.</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">제가 없어도… 괜찮을까요…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">위험한 것 같은데…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">후퇴할 수는 없는… 거겠죠.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">다들… 너무 지친 것 같아요.</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">좋아… 제대로 맞췄어.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">옆에 있는 녀석들도 정상적인 모습은 아니야…</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">이런 건 누구라도 빨리 끝내고 싶겠죠…</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">허, 머리가 세 개나 달려있네…</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">까다롭네… 무시할 수 없는 힘이야.</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">진짜 괴물 같은 놈들이잖아…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">진짜 뭐야 저거… 어떻게 저런 힘을 다루지?</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">골치 아프네… 방법이 보이지 않아.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">어떻게든… 해결할 수 있겠지.</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">아슬아슬했어.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">조금만 더 버티자고.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">좋아. 길이 좀 보이는 것 같지?</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">뒤틀림이라… 분위기가 좋지 않아.</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">조금만 더 끌었다면 위험했을 거야…</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">죄송합니다… 더는 버틸 수가…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">남은 녀석들이라도 조심해.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">제길… 전혀 만만하지 않잖아.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">어떻게 하면 힘을 못 쓰게 만들 수 있을까…</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">성가시네… 얼마나 남았지?</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">다 처리할 수 있을까…</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">승산이 아예 없는 건 아니겠네…</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>