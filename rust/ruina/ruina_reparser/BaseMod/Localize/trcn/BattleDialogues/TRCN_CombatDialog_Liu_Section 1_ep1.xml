<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>ep1</GroupName>
  <Character ID="Chun">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">冷靜地開始吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">絕不能讓敵軍壓制我等。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">對手若是難以撼動，那便轉而回防。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">如科長所言，全員不得自亂陣腳。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">百戰百勝，不足掛齒。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">雖說巧妙，持久戰絕非上策。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……科長大人，恐怕我無以為繼了。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">拉長戰線只會更加危險。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">前進。不容後撤！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……敵軍的態勢不容小覷。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">猛虎伏草……即使全員陣亡，我也信任科長大人。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">出其不意，攻其不備……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">千萬不要露出破綻。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">堅固的防守，便是守住無法被攻擊到的地方。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">若是前途渺茫，又怎會來到這裡。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Liu_Fixer_Sec1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">一心向前。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">不露後背！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">化作名譽豪氣雙收的戰場巨龍！</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">無論何時都要身心正直。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">一如既往，始終如一。</BattleDialog>
      <BattleDialog ID="START_BATTLE_5">無所畏懼。衝鋒！</BattleDialog>
      <BattleDialog ID="START_BATTLE_6">掃六合，定天下。</BattleDialog>
      <BattleDialog ID="START_BATTLE_7">兵貴神速！</BattleDialog>
      <BattleDialog ID="START_BATTLE_8">椿大人，就這樣向前衝嗎？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">憑藉這種實力走到了今天……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">總之目標達成了，回去吧。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">即使獲勝也不得驕傲自滿。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">這是他們咎由自取。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">大獲全勝。收集好委託的書籍就回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我可不想當個拖後腿的……</BattleDialog>
      <BattleDialog ID="DEATH_1">還不想就這樣放棄……</BattleDialog>
      <BattleDialog ID="DEATH_2">無謀……一念之差……</BattleDialog>
      <BattleDialog ID="DEATH_3">猶豫就會落敗……</BattleDialog>
      <BattleDialog ID="DEATH_4">已經……到極限了。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">沒時間顧及個人感情了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">這種情況下絕不能崩潰。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">人都死了誰能咽下這口氣啊。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">繼續展開攻勢！萬萬不可坐以待斃。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">臨危不懼，永不後退。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">攻敵必所救。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">一鼓作氣。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">用安逸的心態戰鬥只會是這種下場。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">絕不放過一瞬，絕不錯漏一人。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">只有手中的武器是值得託付的。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_5">這種攻擊是想抓癢嗎。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Xiao">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">主宰敵軍的性命！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">我有克敵制勝之計。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不用管我，若是危險就走為上策。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">感謝各位隨我至此。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">勝利了，也並未勝利。海市蜃樓，鏡花水月。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">！！！</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……剩下的人一定要保重自己。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">打起精神來！還沒有結束……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">我絕不會被擊垮……繼續前進。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">點到為止！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">精誠所至，金石為開。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">速戰速決。全員集中！</BattleDialog>
    </Type>
  </Character>
  <Character ID="Miris">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">逞強只能趁現在啦！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">可別陷入不利戰局囉。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">壓掉損失，趕緊完事！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">真是萬幸……帶著書回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">他媽的……太危險了……科長大人！</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……死灰也是會複燃的。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">未竟之役，切忌灰心喪氣！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">一定有什麼……能夠逆轉這場戰鬥。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">科長大人！下一道指示是…！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">在科長大人的統帥下，我們戰無不勝！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">所剩無幾了！！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">敵軍自亂陣腳了，機不可失！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">展開更猛烈的攻勢吧！</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>