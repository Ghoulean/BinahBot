<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>2</GroupName>
  <Character ID="Esther">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">緊閉雙眼，莫被無謂之物動搖心智。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">各守其位，準備迎敵。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">吾輩隨時代行指令之意。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……或許，該去見見那位新傳令了。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">指令永遠指引吾輩邁入正道。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">無可厚非。指令並未要求得勝……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">渴望被賦予意義之人，理應胸懷赴死之決心。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">即刻動身，完成指令。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_2">太陽的高度也會出現偏差。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_3">時刻謹記，麻痺大意將招致死亡。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_4">餘下人員繼續執行指令。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">無需辯解。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">到此為止。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">……即將完成。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hubert">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">唯有指令，知曉此地的生死。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">……牽制對手。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">他們的實力無法預測。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">根本不是對手。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">……下一則指令等著我們。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……這便是既定的結局，我全然接受。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">會不滿於指令規劃的命運嗎。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">命運使然。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_2">不要被眼前的事物衝昏頭腦。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_3">既定之路，已無法改變。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_4">我們必須完成使命。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">可笑。這便是追隨自由意志的結果？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">一氣呵成……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">……還算棘手。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Gloria">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">陽也太過分了~怎麼把我們送進這種地方呀~！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">今天也來好好執行指令吧~</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">指令啊~總能把我們引向正確之地呢。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">都解決了，不費吹灰之力哦~</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">指令有說拿走書嗎~？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">雖然很傷心~但我無可奈何了呢……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">就這麼結束了，真可憐~</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">剩下的人也要繼續執行指令才行喲~？</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_2">這也是試煉的一環嗎~</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_3">真好奇我的極限在哪裡~</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_4">指令不會把我們引向歧途的~</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">你跟剛才那些半吊子一樣，笨手又笨腳~</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我沒有惡意哦~這也是指令的意思嘛。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">這則指令也快完成了呢~</BattleDialog>
    </Type>
  </Character>
