<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Carnival</GroupName>
  <Character ID="Alpha">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">開始嗎？看起來不弱，小心嗎？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">看起來，很美味嗎？但他們，不是人類。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">你們和，清道夫，一樣難吃嗎？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">吃掉書嗎？織出，更美的絲綢嗎？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">快點走嗎？收到新指令。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……味道，不太好。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">跑太快嗎？會被打倒。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">悲傷嗎？一起工作的同僚，死了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">不危險嗎？還能應付。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">輕易看破招式，很簡單嗎？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">必須儘快消除，麻煩嗎？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我不會讓你，打中。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Beta">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">機會來了，能捅到嗎？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">看起來很強嗎？沒問題。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">好奇味道嗎？看起來硬，但好像很好吃。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">興奮嗎？勝利，心情愉悅嗎？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">我們很強嗎？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">再也不能，織布嗎？</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">誰被打倒，要想辦法嗎？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">這樣下去，會輸嗎？要小心。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">必須謹慎嗎？或許，有大麻煩。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">吃掉書，織造新布料嗎？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">你的力量，不足以在我身上，留下傷疤嗎？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">還沒結束，剩下幾個嗎？</BattleDialog>
    </Type>
  </Character>
  <Character ID="Gamma">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">不喜歡受傷嗎？不能站著，挨打。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">必須做這個，然後織出新絲綢嗎？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">減少不必要動作，節省時間嗎？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">未受嚴重損害嗎？疲憊，休息嗎？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">剩下交給食指，快速離開嗎？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">不能嗎？工作尚未，完成……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">變成書嗎？死掉就會，變成那樣。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">沒關係，這樣下去，我們會被重創嗎？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">隨意出手，會有危險嗎？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">殺掉一個嗎？幹淨利落。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">覺得我們，能贏嗎？理所當然。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">幹掉一個，不能掉以輕心嗎？</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>