<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Cane Office</GroupName>
  <Character ID="Nemo">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">哈！哈！哈！看來對方可不好對付呢。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">哎呀，我們以後會經常見面的，還請多多關照呀！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不用擔心！我沒道理會輸。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">這些傢伙長得比我預期中的要醜呢。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">竟能這麼簡單就獲取高價情報！我以後還會經常光顧喲！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">那瘋子究竟在盤算什麼……這種地方……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">沒關係！我還好得很呢。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">哎呀，小心別受傷啊！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">一定要足夠謹慎才行！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">動作太慢囉，想看丟你的動作都很難啊。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">掌握奇點技術的秘密……只是時間問題了！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">太簡單啦！再努力加把勁就能結束了！</BattleDialog>
    </Type>
  </Character>
  <Character ID="Martina">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">老闆……請不要太激動……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">……如果當初有仔細留意契約上的內容……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">這種事情我們以前也做過……不要慌……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">關於W公司的情報……都在這本書裡……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">這下可以喘口氣了……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">就這麼……死了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">辛苦了……抱歉沒能攔住他們……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">死後會變成書……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">被逐漸逼進死角了……仔細觀察情況……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">為達目的……必須這麼做……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">沒剩幾個了……再努力一下……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">處理好這個，之後還有很多事情要做……</BattleDialog>
    </Type>
  </Character>
  <Character ID="Bada">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">老闆說的對，我們應付得來的。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">千萬不要衝動行事，冷靜地看向前方。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">拿到書後回去就行了，沒什麼難的。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">大家沒事嗎？確認狀態非常重要。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">我拿到書了，我們回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">一定要回去……一定要……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……為你默哀，安息吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">是我不夠強。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我們必須努力活下去……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">烏合之眾……不難應對。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">根本不費工夫。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">看來已經結束了。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>