<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Binah</GroupName>
  <Character ID="B_E_Elena">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">讓我看看……你們還留有待趨品嘗的血管嗎？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">忍受乾渴的痛苦……並不美妙。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">一場不盡人意的晚宴。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">這份乾渴……如何擺脫……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">不過是具空殼罷了。絲血未留……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">死亡是公平的……正與每個人起舞呢。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">被吞噬的命運毫無差別地照耀著我們。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">雖然很可惜……但這不是我想要的味道。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">你們和我都是為滿一己私欲而乾杯的，不是嗎？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">啊……就憑這樣也想阻止我？</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Orange">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">……</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Binah">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">不必過度擔憂。全神貫注為好。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">看來這裡結束了。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">只是能力尚且不足。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">堅持至此已屬不易。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">或許你已竭盡全力。辛苦了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">沒有人會斥責你。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">即使只是一具空殼，仍然願你安息。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">這一次，務必沒入永眠。</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">無論如何……既然對方是來賓，就要認真接待。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">辛苦了……有點擔心其他樓層的人。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我絕不後退……！</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我會抗爭到底……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">只要還有一個人站著，就沒有結束。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">不要著急……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">這樣一來，事情能簡單許多……</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">那個血魔……也變成書了？</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">居然能操縱特色……可惡，這下豈不是……？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">大家都還好嗎？有異常情況請立刻告知。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">抱歉……我撐不下去了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">剩下的人要更加謹慎才行。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不能讓迄今為止的努力，化為泡影……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我還撐得住……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">稍慢一拍就出大事了……</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">血魔的力量……都能達到這種程度嗎？</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">朱紅……十字。錯不了的。那塊巨大的十字架……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……心裡真不好過。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">還有敵人……沒有……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……大意了嗎。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不要貿然行動。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">別在這裡停下。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">哪怕成為空殼，力量也絲毫未減。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">總感覺頭有點暈。</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……她身旁的那個人是誰？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我都快累死了……晚點整理吧？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">連我也……被打敗了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">還有別的辦法嗎……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不能因為對方人少就大意輕敵，他們絕非等閒之輩……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">這種局面怎樣才能扭轉……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">居然有一天要面對傳說中的特色……</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">真的……不是一般人能夠承受的力量。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>