<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Netzach</GroupName>
  <Character ID="N_B_Bremen">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">汪汪！咯咯咯~嚶嚶嚶~！！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">咯~咯~！！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">嚶嚶。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">咯……咯……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">汪！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">汪汪汪！！汪汪汪！！！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">咯~！！咯咯！！！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">嚶嚶嚶~！！！</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Netzach">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">嗯……不試試怎麼知道。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">不太……滿意。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……這次也沒幫上什麼忙。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">似乎變危險了……大家的神經再繃緊點。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">大家都在為生存而奮戰吧……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我不會再逃了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">情況……看起來不妙。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">這種事已經習以為常了……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">哈啊……精神不太妙。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">吵鬧的聲音逐漸消失了……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">再加把勁……</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">嗯，一路堅持到現在，沒道理阻止不了他們吧。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">Netzach先生……其他樓層不要緊吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">不妙。再這樣下去……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">這真的，不要緊嗎……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">現在依然有勝算。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……越來越多了。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">必須把他們全部處理掉。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">照此下去……沒有什麼是不可能的。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我大概明白……該怎麼做了。</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">N……Netzach先生，我們真的是對手嗎？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">能順利結束真是萬幸。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">你們沒有我……也沒關係的……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">好像很危險……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">這次不能後退……對吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">大家……好像都累壞了。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">很好……打中了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">身旁那些傢伙也不像是正常人……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">這種事情誰都想儘快結束吧。</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">呵，這傢伙長了三個腦袋……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">贏得真艱難……無法忽視的力量。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">真是一群怪物……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">搞什麼……怎麼能使出這麼大的力氣？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">頭好疼……完全想不出辦法。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">無論如何……都能解決的吧。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">真驚險。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">再堅持一下。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">很好，似乎有些眉目了？</BattleDialog>
    </Type>
  </Character>
  <Character ID="N_B_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">這就是扭曲嗎……氣氛不太妙啊。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">再拖下去可就大事不妙了……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">對不起……再也堅持不下去了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">剩下的傢伙都小心點。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">媽的……太難打了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">怎樣才能將他無力化……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">真煩人……還剩多少？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">能幹掉嗎……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">並非完全沒有勝算……</BattleDialog>
    </Type>
  </Character>
