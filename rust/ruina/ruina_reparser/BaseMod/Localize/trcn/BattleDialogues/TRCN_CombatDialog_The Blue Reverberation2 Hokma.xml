<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Hokma</GroupName>
  <Character ID="H_P_Pluto">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">準備完畢了的話……就開始吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">最好別被他人展現的世界所欺騙。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">希望彼此之間不會產生怨恨。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">又一次……被欺騙了嗎？</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">不要完全相信你的眼睛看見的東西。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">請諒解。我沒有惡意。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我不過是在行我應行之事。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">似乎越來越近了。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Hokma">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">也並非是不可抵擋的洪流。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……只能相信到最後。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">後面就交給其他的人吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">不必自責，安心地睡吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">不應為其所動搖。請各位冷靜地平定呼吸。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">我們所有人都已經踏上了無法止步的道路。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">您所言極是，Hokma先生……有什麼對策嗎？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">說我不氣憤……也不過是虛張聲勢……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">其他人都還好嗎？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不必擔心我。我很清楚自己的能力。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">要是一蹶不振的話才是真的結束了……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">能平安度過這一劫真是萬幸。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">似乎無法透過正常的方式進行戰鬥……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">對不起，沒辦法再繼續幫助你們了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">相信我……然後前進吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">必須得確認一下自己的狀態。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我始終相信……情況是會逆轉的。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……真的結束了對嗎？</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">要相信自己能贏……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">竟然……耍這種手段。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">快要結束了……再堅持一下……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">所以為什麼要如此魯莽地站出來……！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">很擔心……我們真的能做到嗎？</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">好險好險……要是我們沒能阻止他的話會發生什麼呢。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">急功近利會招致禍亂……我會準備好的，</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">再這樣下去……就危險了啊……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我想要是能再集中一點的話會更好吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">將謊言……展現給我看。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">這樣會遭殃的……大家都小心點。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">也不知道相信這東西到什麼程度才算是真相呢。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>