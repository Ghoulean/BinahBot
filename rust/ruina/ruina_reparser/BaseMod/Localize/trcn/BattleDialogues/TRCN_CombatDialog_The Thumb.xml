  <Character ID="Denis">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">都做好準備……沒必要讓上級親自出馬……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">不必焦慮……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">這裡不像別處那樣野蠻……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">不錯……正當的回報……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……我無話可說。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">死後不要記恨……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">專注於手頭的工作……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">尚無危險……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">既然雙方已公平交易……還望您不要怨恨……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">獻上最後的敬意……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">但願您已竭盡全力……</BattleDialog>
    </Type>
