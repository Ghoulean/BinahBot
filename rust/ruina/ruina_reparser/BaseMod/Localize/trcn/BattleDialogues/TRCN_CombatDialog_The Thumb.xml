  <Character ID="Thumb_NoBoss">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">閒聊就請到此為止吧，您還有別的話想說嗎？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">還請您不要猶豫，直接出手吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不論如何，失言必失禮。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">他們的階級遠在我們之上……要遵守禮儀。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">恪盡職守便可……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">任務結束，回去報告吧。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">肩膀有點酸……他們可真不好對付。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">有了這本書……應該就能制服那些傢伙。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">真是棘手的敵人……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">這裡的情報應該大有用處。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……他媽的……就這麼，結束了。</BattleDialog>
      <BattleDialog ID="DEATH_1">……</BattleDialog>
      <BattleDialog ID="DEATH_2">生命是公平的……不應為死亡而怨恨……</BattleDialog>
      <BattleDialog ID="DEATH_3">……我盡到了自己的職責。</BattleDialog>
      <BattleDialog ID="DEATH_4">落得如此境地……只能怪自己太過弱小……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">無論情況如何，都應直截了當。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">無需憐憫，他盡到了職責。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">顧好自己便可。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">若在最後失言，必將醜態盡現。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">暫無大礙。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">手臂開始酸了……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">儘快了結對雙方都有好處……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">畢竟，雙方各有各的職責。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">手下留情並不符合禮儀。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">不竭誠相待可不行。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Kalo">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">即使是敵人，對方也仍貴為都市之星……要遵守禮儀。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">他們沒有拒絕我們的提議。我們應當感到榮幸。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">互相尊重，委實美妙。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">毫無雜質的回報。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……最後一刻，仍應保持禮節。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">無可奈何。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">成敗不得歸罪於個人。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">莫被此等瑣事動搖心智。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">雙方已達成毫無缺憾的交易。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我們只是履行職責。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">窺見勝利之時，不應猶豫。</BattleDialog>
    </Type>
  </Character>
