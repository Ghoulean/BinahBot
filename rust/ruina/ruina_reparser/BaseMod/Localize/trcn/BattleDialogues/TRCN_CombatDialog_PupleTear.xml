<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>PupleTear</GroupName>
  <Character ID="PupleTear">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">來吧……讓我試試你們的實力吧？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">真好奇圖書館到底擴張了多少。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不趕時間，慢慢來吧。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">實力尚淺，看來時候未到……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">有點意思。但是……還差得很遠呢。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">很好……你們漸漸變強了呢。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">你的動作不夠俐落。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">哼，低於我的期待。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">能阻止我到這一步，值得嘉獎。但是可惜了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">小傢伙，下次再見面時再多做點準備吧。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">做得很好。我已經很久沒在戰鬥中被壓制了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_5">哎呀，真是大言不慚……你的本事只有這樣嗎？</BattleDialog>
    </Type>
  </Character>
  <Character ID="PupleTear_MeetBinah">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">嘛……您會在這裡，倒是一點也不令人驚訝。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">我已經見過你好幾面了，那麼……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">即使擁有那種存在，結局也沒有區別。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">哈哈……真是有趣……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">我還以為這次會有所不同。</BattleDialog>
    </Type>
  </Character>
  <Character ID="PupleTear_MeetGebura">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">你不能說是與卡莉相同的存在呢，對吧？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">你還記得曾經的教誨嗎……來看看吧？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">卡莉。向你學習的那些傢伙還挺難纏的呢。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">還記得蠻清楚嘛……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">看來你的力量大不如前……</BattleDialog>
    </Type>
  </Character>
  <Character ID="PupleTear_MeetRoland">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">什麼都別說。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">幹的不錯呢。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">你只能做到這樣嗎？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……是啊。這樣才有送你過來的價值。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">你可真是……太懈怠了。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Binah_MeetPupleTear">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">是啊。而我亦知曉你。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">果然如此……不得不大吃一驚呢。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">無法承受她的力量是理所當然的。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">無妨。安心休憩吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">堅持至此值得嘉許。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Gebura_MeetPupleTear">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……別說廢話。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……她很強大。而且動作還有股熟悉感……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">你們還堅持得下去吧？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……即使是特色，也沒什麼差別。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">別自亂陣腳。越是急躁越會產生破綻。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Roland_MeetPupleTear">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……哈。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……你如果是因為擔心我而找上門來，那可就多此一舉了。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……不要氣餒。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">繼續推進。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>