<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Liu</GroupName>
  <Character ID="Liu-Fixer">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">凝視前方，勇往直前。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">我們絕不背向敵人。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">化身為，光榮豪邁的戰場之龍……</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">無論做什麼，都要端正心態。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">這跟以往接手的委託並不相似，但一定能以同樣的方式處理好。</BattleDialog>
      <BattleDialog ID="START_BATTLE_5">照常發揮就好……不要緊張。</BattleDialog>
      <BattleDialog ID="START_BATTLE_6">我不怕，上吧！</BattleDialog>
      <BattleDialog ID="START_BATTLE_7">我們六協會不會輸給任何人。</BattleDialog>
      <BattleDialog ID="START_BATTLE_8">迅速展開攻勢。</BattleDialog>
      <BattleDialog ID="START_BATTLE_9">絕不能給大夥添亂。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">這些傢伙竟能走到這一步，實在難以置信……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">無論如何，目的都已達成了……大夥快回去吧。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">雖然勝利了，但請不要得意忘形。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">我不會憐憫對手。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">順利取勝，拿上委託要求的書回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我討厭，拖大家後腿……</BattleDialog>
      <BattleDialog ID="DEATH_1">不想就這樣……放棄……</BattleDialog>
      <BattleDialog ID="DEATH_2">我大意了……一著不慎全盤皆輸……</BattleDialog>
      <BattleDialog ID="DEATH_3">猶疑不定只會成為阻礙……</BattleDialog>
      <BattleDialog ID="DEATH_4">看來……我已經撐到極限了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">沒時間顧慮個人情感。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……我們只能默默前進。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">親眼目睹同事死去，我冷靜不了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">……繼續展開攻勢。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">哪怕危機四伏，我們也決不退縮！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">希望能像方才那樣順利了結。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">以那種心態迎戰，這就是你的下場！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我不會錯失任何一刻，也不會放跑任何一人。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">現在能依靠的，唯有手中的利刃。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">那種軟弱的攻擊傷不到我。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Lowell">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">保持專注，這些人不可小覷。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">塞希爾、梅，各位可有準備好？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">決不鬆懈，我等必將凱旋。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">不要怕。戰勝自己，然後戰勝敵人。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">各位辛苦了，回收完畢後立刻返回。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">敵方潰不成軍，勝利屬於我等。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……哪怕結局如此，但我堅持到了最後。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">抱歉……辛苦了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我會繼承你的遺志……我們一定會得勝。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……該撤退嗎？但現在不是時候……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">情況危急，各位加強防禦。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">哪怕所有人都已倒下，但只要我一息尚存……就有勝算！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">戰鬥遠沒有結束，繼續保持專注！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">向前推進，還沒有結束……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">哪怕訓練有素，總會露出破綻。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Cecil">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">是！準備應戰。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">堅定向前，不要露出後背！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">趕在敵人組成陣型之前，先發制人！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我們收集到了不少有利情報。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">不知道1科是否順利完成工作，我們回去吧？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">以我的實力，要想戰勝都市之星……還遠遠不夠嗎？</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">繼續行動！注意腳下！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我還撐得住，絕不能輸……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">端正心態……我們能挺過去。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">動作再狠一些！！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">乾淨利落的收尾。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">沒剩多少了……一同衝入敵陣！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">熱血上湧，狀態良好！</BattleDialog>
    </Type>
  </Character>
  <Character ID="Mei">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">準備好了喲！馬上出戰。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">敵人的數量並不算多嘛！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">知己知彼！百戰不殆！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我才剛剛熱完身耶……真遺憾喲！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">他們的陣型揮手即破！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">科長先生，對不起……我本該，堅持下去的……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我不會停下！絕不能辜負死去的同事！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">戰鬥還……沒有結束！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">別讓他們看扁了……我們上！！！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">我不會蠢到連這點心理準備都沒有就踏上戰場。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">順利解決囉！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">大家都沒事吧？要知進退喲。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">戰鬥早就開始了，現在才拿出幹勁可太遲了喲！</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>