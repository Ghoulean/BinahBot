<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Malkuth</GroupName>
  <Character ID="M_P_Philip">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">已經沒什麼好說的了吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">……我沒有任何惡意。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">請看。你們就像曾經的我一樣。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">不過是再次回歸而已……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">…………</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">與我不同，你們故步自封，一成不變。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">願你不會對離開自己身邊的人有所留戀……</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Malkuth">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">不要緊張，穩紮穩打地前進吧！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">心情並不怎麼舒暢。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……不要放棄。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">沒關係，無需自責。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我們能做到的……相信我！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">退無可退了……奮戰到底吧！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">你已經很努力了！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">好……就這麼走下去吧！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">好像還遊刃有餘呢？</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Malkuth小姐！現在該怎麼辦才好……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">結束了……整理好再回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">不……我需要更多援助……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">稍微有點可怕了呀……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">情況僵持不下。再多想想辦法……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">除了迎頭而上別無選擇了嗎……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">那些天使的雕像還是老樣子。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">得速戰速決……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">其他人沒事吧？</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">都變了許多呢。我們也是，那傢伙也是。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">落幕的味道五味雜陳。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">媽的……可不能就這麼結束啊……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">更危險了啊……得多加小心了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">只靠我們自己的力量……能夠攔住他嗎？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">那個樂團怎麼會有如此程度的力量……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">不要再拖延時間了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">畢竟是會過一次的對手……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">感覺還挺順利的嘛？</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">比起那時陷入暴走的你，應該更難對付了吧……？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">哈啊……真不是個簡單的對手……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我不想變成累贅啊……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……看來應該改變戰術了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">再這樣下去，我們樓層會……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">怎麼辦……該怎樣才好……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">現在還留有餘裕呢！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">照這個步調推進就好！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">馬上就來幫你！</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">和過去不同，似乎沉穩了許多。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我們真的贏了嗎……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">還得……堅持下去……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">別擔心，我不要緊。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">抱歉，應該注意你的後背才對。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……剩下的就交給我們吧。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">作戰方式好像並沒有太大的變化……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">又是那種雕像……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">如果整個樂團的力量都達到了這種程度的話……</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>