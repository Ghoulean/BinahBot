<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>HanaAssociation</GroupName>
  <Character ID="Hana_Fixer_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">這裡看起來還真奇怪。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">跟報告中描述的一致。雖然這裡……看起來並不像間圖書館。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">這些人的服裝看起來相當眼熟。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">他們就是以這種方式搶奪來賓的力量？</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">總之……以後會經常見面的。請多指教。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">確實相當棘手……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">這些書籍將有助於進一步瞭解圖書館。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">回去之後有很多報告要寫。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">科長她還好嗎……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">局勢好像穩定住了……回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">大意了……</BattleDialog>
      <BattleDialog ID="DEATH_1">這種結局，還是無法避免嗎……</BattleDialog>
      <BattleDialog ID="DEATH_2">只靠我們的力量……果然……</BattleDialog>
      <BattleDialog ID="DEATH_3">最起碼要牽制住他們……</BattleDialog>
      <BattleDialog ID="DEATH_4">雖然很不甘心……但現在，是時候放棄了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……我要竭盡全力。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我們會沒事的……一定。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……其他人狀態如何？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">就快結束了……大家再堅持一下。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">還不算致命。至少目前如此……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">有破綻。看來你開始急了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">沒剩幾個了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我方也並不輕鬆。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">我不會失手！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">繼續留意四周。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hana_Fixer_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">美里奈小姐，可以開始了嗎？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">不用擔心。我們會妥善處置並活下來的。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">遵命，我會在一旁輔助進攻。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">無論如何……今後還會經常見面。請多指教。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">我會照常進行。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">確實相當棘手……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">似乎並無大礙。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">大家都還好嗎？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">這樣的委託每次都讓人心驚膽戰……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">我覺得還行……各位檢查一下自己的傷勢。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我……不怨誰。</BattleDialog>
      <BattleDialog ID="DEATH_1">很抱歉……我撐不下去了……</BattleDialog>
      <BattleDialog ID="DEATH_2">只靠我們……果然還不夠……</BattleDialog>
      <BattleDialog ID="DEATH_3">大意了……應該更謹慎的……</BattleDialog>
      <BattleDialog ID="DEATH_4">是我疏忽了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">對我們還不算致命。至少目前如此……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……我要盡我所能。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我們會沒事的……一定會的。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">……其他人狀態如何？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">既然無路可退，那就堅持到底。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">有破綻。看來你開始急了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">沒剩多少了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我方也並不輕鬆。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">不會失手的！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">繼續警戒周圍。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hana_Mirinae">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">精神集中。尚不知曉對方的實際戰力。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">展現你們的成果吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">希望各位充分發揮。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">希望你們不會輕易認輸。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">要想得到某物，就該做好失去某物的覺悟……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">保持平常心。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">一旦失敗，我們的命運也會就此終結。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我們也並非毫無對策地闖進來。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">連預測的必要都沒有。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">直到最後都不要放鬆警惕。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">看來你們低估了我們。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hana_Harold">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">距離很近……先觀察對方的動向。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">相較其他都市之星，這裡並沒有什麼不同……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">美里奈科長。準備好了嗎？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">沒什麼難的……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">已經成為高懸於夜空的星了嗎……</BattleDialog>
      <BattleDialog ID="DEATH_1">吞噬一切的力量……比想像中……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">不得退縮……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">剩下的人都打起精神。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……嗯。這樣下去可不妙。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">但願能像這樣順利解決……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">……就快結束了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">每個人都強得不可小覷……</BattleDialog>
    </Type>
    <Type ID="ERROR" />
  </Character>
