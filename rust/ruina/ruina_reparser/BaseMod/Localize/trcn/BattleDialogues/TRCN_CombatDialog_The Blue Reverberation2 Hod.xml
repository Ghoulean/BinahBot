<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Hod</GroupName>
  <Character ID="H_G_Greta">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">喀喀喀！那就讓咱開始料理吧？！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">是非常有意思的食材啊！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">也不知道其它夥計們的工作做得怎麼樣囉！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">這還真是……別具特色的美味……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">這傢伙……不能拿來做菜嗎？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">真沒勁！全都是些一口就能吃掉的貨色。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">味道不怎麼爽口！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">想到新的烹飪技巧了！</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Hod">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">一直以來我也透過你們學習到了很多。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……即使如此，哪怕能為大家提供一點幫助………</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我沒有大礙……其他人還好嗎？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……得找出突破僵局的方法。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">啊，嗯。沒關係，我不會倒下的！</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">我並不想要踐踏你的夢想，只是……</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Hod小姐，您還好吧？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……自食惡果。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">不行……越是輕率越會不利。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">還沒有完全陷入困境。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">必須看穿她的動作……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……似乎度過了一個難關呢。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">現在可不是擔心別人的時候啊。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">無能為力……交給你們了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">只能冷靜地……繼續前行。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">真是蠻不講理……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">情況還沒那麼糟……大概吧……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">萬幸的是，我們終於終結了這一切……</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">真擔心別的樓層……不知大家是否都相安無事呢……？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我想……我已經盡全力堅持了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">很抱歉。現在能為你做的只有……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">要是一時大意……我也會……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">情況還沒有失控……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">無論如何，總算是阻止了她……</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">這到底有幾張嘴呀……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">他媽的……大意了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">慎重看待這次死亡並做出對策吧……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">那頭大白鯊……也太蠻橫了吧？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">難不成真的是將我們當做食材來宰殺嗎……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">哈啊……那把沉甸甸的武器實在是有點嚇人。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>