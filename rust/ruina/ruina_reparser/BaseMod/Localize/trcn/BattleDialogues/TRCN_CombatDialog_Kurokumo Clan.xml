<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Kurokumo Clan</GroupName>
  <Character ID="Sayo">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">這些可愛的小傢伙都是從哪裡找來的~？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">一眨眼就會結束的。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">如果非要擊敗我們……各位，做好準備。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我想，這次你們應該吃到些教訓了吧？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">如果乖乖卷起尾巴投降，就不會發生這種事了……唉，真可惜。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">要不是那個狗東西，事情本該很順利的……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">嘖，就這點本事還敢說要保護我？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">這可不行……你的使命還沒完成呢。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">情況的確令人擔憂呢。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">實力不足就趕快退場吧。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">感到不安嗎？怪不得看起來如此焦急~</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">和傳聞所說的相差甚遠啊。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Gin">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">這邊交給我。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">作為敵人還是有點棘手。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">即便勢均力敵，也不可鬆懈。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">真是一場苦戰，累慘了。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">都收拾乾淨了，快回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……都怪我實力不足，對不起。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">本來不該有這種無意義的犧牲。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">本身就消耗了不少精力……快到極限了嗎？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">可不能再有人倒下了。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">就憑那點本事，還想阻止我們？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">學學量力而行的道理。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">很遺憾，放棄吧。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Yang">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">呼~好累，好累。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">什麼時候才能不必沾上血污呢？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">這麼慌張，可沒什麼好處喲~</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">難怪那傢伙要把我們送進來？這陣子怪事可真多。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">太弱了，實在看不下去。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">媽的……你們可知道這樣對待黑雲會的人會有什麼下場……？</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">靠……攻勢可真猛烈，得小心了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不像話，連這種廢物都打不過。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">這樣一個一個地損失下去，可不是什麼好兆頭……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">哈，我可是給足你機會囉。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">這可不是兒戲喲。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">這種程度的攻擊，根本沒有躲的必要。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Normal Enemies">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">休想碰紗世大小姐一根毫毛。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">敢和黑雲會叫陣……真是狗膽包天。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">我來打頭陣，大家準備好。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">好咧，打完收工。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">真想在熱乎乎的錢湯好好洗掉這身血污啊。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我怎麼會……犯這種錯……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">快，快撤吧，這裡不對勁……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">死了一個……情況不妙。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">後背涼颼颼的，該死……真可怕。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">還沒結束，注意點。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">實力不錯……但這最後一手太魯莽了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我會把你們一個個殺乾淨。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>