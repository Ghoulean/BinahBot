<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Sweepers</GroupName>
  <Character ID="Anton">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">我們……現在……開始掃除……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">向前……推進……片刻……不停……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">這片空間……似乎……還有……另一面……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">現在……家人們……都能……好好吃上飯……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">希望……鄰居們……能再……加把勁……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">儘快……掃完……還有……很多……很多……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">無論誰……倒下……我都要……繼續向前……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我……沒問題……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">沒有……食物……他們……變成了書……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我已經……很久……沒有……動手清掃……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">只要……將攔路的事物……全部……掃除……</BattleDialog>
    </Type>
  </Character>
  <Character ID="Valerie">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">各位鄰居，燃料是否充足？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">和家人一起，必能排除萬難。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">為了孩子們，一定要拿到這裡的書。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">把掉在那兒的書收回去，媽媽一定會高興的。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">各位鄰居……請務必……小心……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">為了留在村裡的家人，說什麼都不能停下。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">將眼前的一切盡數掃除。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……有位鄰居死了。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">只是像往常一樣進餐罷了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">沒有溶化，他們不是人。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">各位，再加把勁。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Lyla">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">家人和鄰居都在，我很安心。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">為了不讓媽媽生氣，我們只能靠自己。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">掃完就走，我們還要為今晚的清掃做準備。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">這些書會幫到在L巢進餐的家人。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">希望大家……能平安無事……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">掃除時絕對不能分心。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不用牽掛我們。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">沒時間思考了，繼續清掃。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">這傢伙變成了一本書……沒有溶化？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我沒問題的，傷口不深。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">還有很多東西需要收拾。</BattleDialog>
    </Type>
  </Character>
  <Character ID="sweepers">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">為了我們的媽媽和家人……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">比起後巷，要清掃的東西並不算多……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">一心打掃即可。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">家人的紐帶……非常強大……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">拿上那本書就好了？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">我們快走吧，夜晚還有事情要做。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">鄰居們……抱……歉……</BattleDialog>
      <BattleDialog ID="DEATH_1">還沒有……掃完……</BattleDialog>
      <BattleDialog ID="DEATH_2">家裡的孩子，還在等我……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我還能繼續清掃。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">沒關係，我的燃料還有剩餘。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">繼續前進。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">沒有溶化？怎麼回事？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我沒事，傷口只有寥寥幾道。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">還有很多事情要做。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>