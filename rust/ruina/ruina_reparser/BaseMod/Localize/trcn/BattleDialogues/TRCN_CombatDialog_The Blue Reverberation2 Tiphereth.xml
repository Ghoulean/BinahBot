<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Tiphereth</GroupName>
  <Character ID="T_O_Oswald">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">喔吼吼！演出開始啦~</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">來來來！無盡的笑聲和掌聲在哪裡！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">感謝陪伴我們演到最後一刻的小丑朋友們……請務必獻上熱烈的掌聲！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">那是只屬於我的燈光……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">天天天天呐……我們的團員……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">為什麼只讓我們發笑！！！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">嘻嘻，這位小丑朋友太~有意思啦！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">興奮得想要滿地打滾吧？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">果然！我們的演出能為大家帶來歡樂！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">唱得再大聲點！</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Tiphereth">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">不要緊。就像往常那樣。都明白嗎？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……大家辛苦了。整理完畢後再作等待吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……不。我一定要做到。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">還沒到癱倒的時候！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">只要信任彼此，就一定可以……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">不會再讓你們為所欲為了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">打起精神來。我們還有希望。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">雖然很累，但再堅持一下！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我一個人也能做到……先操心你自己吧！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">還不賴。大家狀態如何？</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">你不會已經在怕了吧？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">嗯……那幫傢伙也一定能度過難關吧？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">對不起……已經到極限了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">沒關係，剩下的都交給我。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我不會就這麼倒下。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……要全力以赴！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">這邊搞定啦~</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">雖然很難纏……但確實是個有趣的傢伙。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">你們都還好吧？</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">只能……一戰了。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">幸好圓滿結束了。對不對？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">原本該幫上更多的忙……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">越是這種時候……越要冷靜啊。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">長相也好，幹的事情也罷，全都令人作嘔……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">其他人還能堅持下去嗎？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">我也覺得自己像個小丑。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">這邊順利完成了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">那聲音震得我頭皮發麻……</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">怎麼像是踏進了馬戲團……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">能給人帶來歡笑的演出……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我怎麼會……敗在一群小丑手裡……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">會有更好的解決方案……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">就算非常不安……我也會帶著你那份繼續戰鬥的。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">對方不容小覷啊。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">好。就像這樣繼續前進！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">簡直像在……表演一樣。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">長得可真噁心……</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">跟之前見過的那些傢伙差不多。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">Tiphereth小姐，現在該回去嗎？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">呃……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">不能再讓事態惡化下去。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……這沒道理吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">所剩不多了……再堅持一下。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">這種吵死人的演出真不像話。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">那個小丑只會耍嘴皮子。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我不想跟你鬧。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>