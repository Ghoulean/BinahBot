<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>r_corp_ep1</GroupName>
  <Character ID="R_Corp_Rudolph_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">馴鹿們，快速進軍。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">對手似乎不好對付，都打起精神。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">沒有其它隊的援助，我們也能做到。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">出於禮貌，向各位說聲辛苦了。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……還沒……還沒結束……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我要多加小心。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">漸漸危險了……精神不太正常……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">一切都是精神力量的博弈。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">想阻擋我們還遜色了點。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">馴鹿們。沒剩多少了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">動作快！長期戰鬥對我們不利。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Reindeer_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">今天也要出擊呢。一起努力吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">這突然是怎麼回事……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">我們也沒有其他選擇。弱肉強食。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">嘛，這陣子的所做所為都是為了保住性命。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">一口氣結束吧。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">再拖下去就不妙了。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">這樣就能免於宰殺了吧……？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">不知道。高層會看著辦的吧。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">魯道夫隊長，現在該回去嗎？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">是不是，太小看他們了……</BattleDialog>
      <BattleDialog ID="DEATH_1">希望其它隊能打贏……</BattleDialog>
      <BattleDialog ID="DEATH_2">情報不足……如果能更進一步調查再來的話……</BattleDialog>
      <BattleDialog ID="DEATH_3">這裡也全是怪物……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">要堅持下去……大家都在堅持……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">這……真沒想到……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">雖然知道不能焦慮……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">那種力量……只在深層見到過……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">如果連武器用不好，那便毫無意義。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">目前為止還沒有問題，繼續戰鬥吧。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">……越來越難撐了。得儘快結束才行……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">那幫傢伙好像也快不行了。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Maxim_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">是死是活，全看怎麼選！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">俺們此刻的任務，就是橫衝直撞！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">先打倒那幫人，再想那些有的沒的！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">比處理廢墟裡的東西還簡單嘛！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">沒想像的那麼了不起嘛！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">還沒大鬧一通呢……就這麼輸了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">還剩幾隻？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">對手不好應付。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">哎呀！那傢伙被踩了一腳。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">你的技巧在壓倒性的力量面前毫無用處！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">衝上去，自然能破開一條道路！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">如同犀角般勇往直前！</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Rhinoceros_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">毫不猶豫，勇猛果斷！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">無論對面是什麼，統統會被犀牛踩扁！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">先從哪個開撞！</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">蠻進的犀牛無人能擋！</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">隊長！能馬上開始嗎！？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">這是我們的勝利！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">你以為會贏嗎？真可惜！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">這樣看來，宰殺能再往後推遲了。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">好久沒遇到像樣的對手了！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我們不會就此屈服……！</BattleDialog>
      <BattleDialog ID="DEATH_1">……咳……撐住……！</BattleDialog>
      <BattleDialog ID="DEATH_2">我不會就這麼倒下！</BattleDialog>
      <BattleDialog ID="DEATH_3">該死的……挨打的反倒是我們……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">剛才稍有閃失，就換我遭殃了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">已經習慣了！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">但願不會牽連到整個小隊！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">我不在乎！我還有事情要做！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">你身體太脆弱了！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">那種攻擊傷不了我！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">還沒完！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">敢擋在我前面……蠢蛋！哈哈！</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Myo_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">兔子來吃草囉！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">都交給兔子吧！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">只要有草，哪裡都行！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">飽餐一頓，大滿足。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">呼~累啦累啦，現在回去吧？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">咳……哈哈……大意了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">一隻兔子蹦回了草原。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不管是過去還是現在，這裡都相當危險呢。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">郊區的任務說不定更有意思。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">還有好多草要撕呢！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">死吧，哈哈！這不算什麼。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">外面的怪物比你們難搞多了！</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Myo_Meet_Gebura_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">你還在當那台機器的傀儡嗎？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">以前的我怎麼也沒料到，我的英雄會變成這樣。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">是啊。過去的都過去了。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……這樣也不錯。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">兔子們，豎起耳朵集中精神。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……一如既往。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">一隻兔子蹦回了草原。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">殷紅迷霧的手下……弱得不像話。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">這些年來我也不是閒著沒事幹的。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">讓他們看看，兔子有多頑強吧。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">還記得嗎？活下來的才是強者。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">今非昔比了。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Gebura_Meet_Myo_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">你的廢話還是那麼多，都說完了？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">沒必要知道你如何看待我。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……希望再也不要見面了。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">媽的……該認的還得認……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">相信自己的能力。我們能宰了那些兔子。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">振作起來，還沒結束。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">不想讓那傢伙白白犧牲吧？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">沒有打不贏的理由，大家保持冷靜。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT" />
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">事過境遷，但我的心靈只會變得更加強大。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">你還是像以前一樣散漫。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Rabbit_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">吃乾抹淨！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">是時候在草地上大鬧一番了！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">準備好盡享美食了嗎？</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">哎呀！草該不會不夠吧？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">火力決定成敗！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">哈哈！好久沒像這樣戰鬥了！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">什麼，已經把草吃完了？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">啊哈哈……這樣下去真要死了……</BattleDialog>
      <BattleDialog ID="DEATH_1">只靠兔子真能解決嗎？</BattleDialog>
      <BattleDialog ID="DEATH_2">都會死的……</BattleDialog>
      <BattleDialog ID="DEATH_3">這樣的任務本來就荒謬無比……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">你的運氣真差。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">這裡也充滿了瘋狂！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我們是不是小看他們了？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">好像有點危險？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">即便是都市之星，到頭來也不過是兔子的嘴中草。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">能啃的草越來越少了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">這頓飯還不賴嘛，對吧？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">我還遊刃有餘呢！動作再快點怎麼樣？</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Nicolai_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">都在吵什麼。閉嘴，精神集中！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">兔子們。我們是為生存而來，能否認真點？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不想死就全力應對。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">回去後要報告的事情堆積如山……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">下次見面……可不會這麼輕易倒下的……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">都知道沒時間留意旁人吧？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">兔子們，打起精神來！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">開始奏效了……堅持下去！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">不要放鬆警惕！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">盡是些無名鼠輩。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">雖然稍有難度，但實力依舊可笑。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">沒剩多少人了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">你的驚恐全寫在臉上。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Nicolai_Meet_Gebura_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……繆，你們認識？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH" />
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT" />
  </Character>
