<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>ep2</GroupName>
  <Character ID="Xiao_EGO_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">是啊……事到如今，信任我吧。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……只願各位的努力沒有白費。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……為了我，醉生夢死地離開此處。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">怎會毫無長進。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">絕不重蹈覆轍。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我的道路，我最清楚。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">即使火燒眉毛也不能人云亦云。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">映入眼簾的一切，皆令人憂心忡忡。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_5">沐甚雨，花愈紅。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Miris_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">無怨無悔。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">是啊……走到這一步，靠的可不只是我們的力量。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">手下敗將，談何兵法！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">藏起頭，可遮不了尾！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">機不可失，時不再來！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">火燼灰冷，消散吧。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Xiao_EGO_2">
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……於盲目的我而言，你們便是水上浮木吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">望眼欲穿，不見歸人。覆水難收，寧為玉碎……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">何時可見鐵樹開花。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">縱有千般恫嚇，既已踏上征程，有何畏懼！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">樂昌破鏡，再也無法映照我的模樣……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">僅是珍重之人的碎片也想尋回，人就是如此。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">不論山有多高，終是天下的一座山。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>