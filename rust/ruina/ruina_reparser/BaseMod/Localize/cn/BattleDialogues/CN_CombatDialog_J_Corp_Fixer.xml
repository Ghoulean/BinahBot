  <Character ID="hanahuda">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">别来跟我们赌运，否则会吃亏哟。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">准备好了么？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">好像来了一群有趣的对手。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">真没劲。不过嘛……奖励倒还不赖。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我竟然，会被逼入绝境……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">那家伙完蛋了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">没有悔牌一说。现在不能放弃……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">呵，简单的赌局。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">继续前进。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">慢慢铺展开吧。</BattleDialog>
    </Type>
  </Character>

  <Character ID="JCorpFixer">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">赢也罢输也罢，人生又有何意义呢……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">又要略施小计了……我讨厌这样！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">走吧~！很快就能结束了。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">这场赌局……好像是我们赢了？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">这局相当成功啊！对吧，花札大人？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">公平的回报，我很满意！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">挨了一下……</BattleDialog>
      <BattleDialog ID="DEATH_1">不知道他们是以怎样的心态挣扎的……</BattleDialog>
      <BattleDialog ID="DEATH_2">这是通过正当的赌注所得的结果……我接受。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……不。笑到最后的……一定是我。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">说是会失去宝贵的同事……果然。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">这一局还没有结束。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">来挡住我啊！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">呼呼……照此下去我们会轻松赢得胜利。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">这也太容易了吧？唉！</BattleDialog>
    </Type>
