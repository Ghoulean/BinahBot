<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>ep2</GroupName>
  <Character ID="Xiao_EGO_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">是啊……事到如今，信任我吧。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……只愿各位的努力没有白费。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……为了我，醉生梦死地离开此处。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">怎会毫无长进。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">绝不重蹈覆辙。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我的道路，我最清楚。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">即使火烧眉毛也不能人云亦云。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">映入眼帘的一切，皆是令人忧心忡忡。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_5">沐甚雨，花愈红。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Miris_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">无怨无悔。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">是啊……走到这一步，靠的可不只是我们的力量。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">手下败将，谈何兵法！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">藏起头，可遮不了尾！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">机不可失，时不再来！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">火烬灰冷，消散吧。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Xiao_EGO_2">
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……于盲目的我而言，你们便是水上浮木吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">望眼欲穿，不见归人。覆水难收，宁为玉碎……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">何时可见铁树开花。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">纵有千般恫吓，既已踏上征程，有何畏惧！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">乐昌破镜，再也无法映照我的模样……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">仅是珍重之人的碎片也想寻回，人就是如此。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">不论山有多高，终是天下的一座山。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>