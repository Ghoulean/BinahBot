<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Bayyard</GroupName>
  <Character ID="Named">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">看来这里就是图书馆……要小心。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">正面突破吧！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">狂野之力无人能敌！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">物竞天择，弱肉强食！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">哈哈哈！还不赖嘛！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">今后……谁来带领他们……</BattleDialog>
      <BattleDialog ID="DEATH_1">该低头时，就得低头……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">别被敌人的气势压倒了！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不许后退！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">坚定前行。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">哈哈！这家伙……挺好对付的。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">始终不得掉以轻心，都听明白了吗！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">结果还是没能拦住我嘛。</BattleDialog>
    </Type>
  </Character>

  <Character ID="Bayyard">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">他们人数不多！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">我来打头阵！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">图书馆……可没那么好对付。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">嗯，这种事儿已经处理过很多次了……</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">我们没问题的。做好准备！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">好。回去吧！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">这种程度还远远不能让我屈服！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">回去的路上一块喝一杯吧？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">话说回来，咱能拿到什么书？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">是带有情报的书……但愿能派上用场。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……承认自己的弱点也很重要。</BattleDialog>
      <BattleDialog ID="DEATH_1">他妈的……是我略逊一筹……</BattleDialog>
      <BattleDialog ID="DEATH_2">别管我了……</BattleDialog>
      <BattleDialog ID="DEATH_3">没想到，就这样结束了……</BattleDialog>
      <BattleDialog ID="DEATH_4">我已经尽力了……没留遗憾。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">决不后撤！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">这像什么话。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">情况变得尴尬了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">……即便如此，也不能放弃！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">我还没完呢。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">哈哈！感觉不错！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">没想象中那么苛刻嘛！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">是时候强行推进了！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">终于干倒了！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">破绽太明显了！</BattleDialog>
    </Type>
  </Character>
  
</BattleDialogRoot>