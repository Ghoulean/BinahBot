<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>r_corp_ep1</GroupName>
  <Character ID="R_Corp_Rudolph_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">驯鹿们，快速进军。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">对手似乎不好对付，都打起精神。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">没有其它队的援助，我们也能做到。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">出于礼貌，向给位说声辛苦了。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……还没……还没结束……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我要多加小心。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">渐渐危险了……精神不太正常……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">一切都是精神力量的博弈。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">想阻挡我们还差那么点意思。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">驯鹿们。不剩多少了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">动作快！长线战斗对我们不利。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Reindeer_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">今天也要出击呢。一起努力吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">这突然是怎么回事……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">没办法。难道你想被一起宰杀处置吗？</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">嘛，这阵子的所做所为都是为了保住性命。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">一口气干完吧。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">再拖下去就不妙了。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">我们不会再被宰杀了吧……？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">不知道。高层会看着办的吧。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">鲁道夫队长，现在该回去吗？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">是不是，太小看他们了……</BattleDialog>
      <BattleDialog ID="DEATH_1">希望其它队能打赢……</BattleDialog>
      <BattleDialog ID="DEATH_2">情报不足……如果能更进一步调查再来的话……</BattleDialog>
      <BattleDialog ID="DEATH_3">这里也全是怪物……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">要坚持下去……大家都在坚持……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">这……真没想到……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">虽然知道不能焦虑……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">那种力量……只在深层见到过……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">如果连武器用不好，那便毫无意义。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">目前为止还没有问题，继续战斗吧。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">……越来越难撑了。得尽快结束才行……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">那帮家伙好像也快不行了。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Maxim_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">是死是活，全看怎么选！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">俺们此刻的任务，就是横冲直撞！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">先打倒那帮崽子，再想些有的没的！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">比废墟里的东西还厉害！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">没想象的那么了不起嘛！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">还没大闹一通呢……就这么输了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">还剩几只？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">真是个伤脑筋的家伙。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">哎呀！被踩了。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">你的技巧在蛮力面前毫无卵用！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">冲上去，路自然就有了！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">如同犀角般独立前行！</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Rhinoceros_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">毫不犹豫，勇猛果断！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">甭管对面是什么，统统会被犀牛踩扁！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">先从哪个开撞！</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">蛮进的犀牛无人能挡！</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">队长！能马上开始吗！？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">这是我们的胜利！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">你以为会赢？真可惜！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">这样看来，宰杀能再往后稍稍了。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">好久没遇到像样的对手了！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我们不会就此屈服……！</BattleDialog>
      <BattleDialog ID="DEATH_1">……咳……顶住……！</BattleDialog>
      <BattleDialog ID="DEATH_2">我不会就这么倒下！</BattleDialog>
      <BattleDialog ID="DEATH_3">奶奶的……挨打的是我们这边……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">刚才稍有闪失，遭殃的就换我了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">已经习惯了！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">但愿不会牵连到整个小队！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">我不在乎！还有事情要做！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">你身体好软啊！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">那种攻击伤不到我！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">还没完！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">敢挡在我前面……蠢蛋！哈哈！</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Myo_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">兔子来吃草喽！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">都交给兔子吧！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">只要有草，哪里都行！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">饱餐一顿，超满足。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">呼~累啦累啦，现在回去吧？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">咳……哈哈……大意了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">一只兔子蹦回了草原。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不管是过去还是现在，这里都相当危险呢。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">郊区的任务说不定更有意思。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">还有好多草要撕呢！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">死吧，哈哈！这不算什么。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">那边的怪物应该更不好对付吧？</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Myo_Meet_Gebura_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">你还在当那机器的傀儡吗？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">以前的我怎么也没料到，我的英雄会变成这样。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">是啊。过去的都过去了。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……也不错。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">兔子们，竖起耳朵集中精神。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……一如既往。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">一只兔子蹦回了草原。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">殷红迷雾的手下……弱得不像话。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我也不是闲着没事干的。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">让我们看看，兔子有多顽强吧。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">还记得吗？活下来的才是强者。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">今非昔比了。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Gebura_Meet_Myo_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">废话还是那么多，都扯完了？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">没必要知道你如何看待我。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……希望再也不要见面了。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">妈的……该认的还得认……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">大家伙相信我。我们能宰了那些兔子。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">振作起来，还没结束。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">不想让那家伙白白牺牲吧？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">没有不胜的理由，大家伙冷静。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT" />
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">虽然改变了很多，但你的心灵变得更加强大了。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">你的弱点也还和以前一样。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Rabbit_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">吃干抹净！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">是时候在草地上大闹一通了！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">准备好尽享美食了吗？</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">哎呀！草该不会不够吧？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">火力决定成败！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">哈哈！好久没像这样打了！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">就没草能吃啦？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">啊哈哈……这样下去真要死了……</BattleDialog>
      <BattleDialog ID="DEATH_1">只靠兔子真能解决吗？</BattleDialog>
      <BattleDialog ID="DEATH_2">都会死的……</BattleDialog>
      <BattleDialog ID="DEATH_3">这样的任务本就荒谬无比……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">运气差爆了的家伙。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">这里也满是疯狂！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我们是不是小看他们了？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">好像有点危险吧？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">即便是都市之星，到头来也不过是兔子的嘴中草。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">能啃的草越来越少了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">这顿饭还不赖，对不？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">我还游刃有余呢！动作再快点怎么样？</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Nicolai_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">都吵什么。闭嘴，精神集中！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">兔子们。我们为生存而来，都想活下去吧？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不想死就全力应对。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">回去后要报告的事情堆积如山……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">下次见面……就不再如此了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">都知道没有时间关心周围吧？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">兔子们，打起精神来！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">逐渐奏效了……坚持住！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">不要放松警惕！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">尽是些无名鼠辈。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">虽然稍有难度，但实力依旧可笑。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">不剩多少。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">你的恐惧太过显眼。</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Nicolai_Meet_Gebura_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……缪，你们认识？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH" />
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT" />
  </Character>
