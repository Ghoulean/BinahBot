<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>PupleTear</GroupName>
  <Character ID="PupleTear">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">来吧……让我试试你们的实力吧？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">图书馆到底扩张了多少呢。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">还算游刃有余，慢慢来吧。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">期待的还没到来吗……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">有点意思。但是……还差得很远呢。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">没错……渐渐变强了呢。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">还没有完全掌控自己吗？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">哼，不及期待。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">能阻止我到这一步，很不错啦。但是，可惜了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">嘿，下次再见面时会多做些准备吗？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">还算凑合。很久没被这样围攻了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_5">哎呀，真是大言不惭……到此为止了吗？</BattleDialog>
    </Type>
  </Character>
  <Character ID="PupleTear_MeetBinah">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">嘛……您会在这里，倒是一点也不令人惊讶。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">早就见过好几面了……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">即使有着那种存在，结局也没有区别。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">哈哈……真是有趣……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">本以为这次会有区别。</BattleDialog>
    </Type>
  </Character>
  <Character ID="PupleTear_MeetGebura">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">也不能说是和卡莉一样的存在呢。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">还记得曾经的教诲吗……来看看吧？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">卡莉。向你学习的那些家伙，稍微有些难办呀。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">还记得蛮清楚嘛……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">果然和当年的力量不同吗……</BattleDialog>
    </Type>
  </Character>
  <Character ID="PupleTear_MeetRoland">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">就像那样，什么都不要说。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">干的不错呢。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">这就是你们能做到的最好吗？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……是啊。不到这一步，可不值得让你去啊。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">你可真是……太懈怠了。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Binah_MeetPupleTear">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">是啊。除此之外，我亦是知晓你。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">果然如此……不得不大吃一惊呢。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">你们不过是缺乏力量的依凭。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">无妨。安心休憩吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">即便如此，依旧坚持许久。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Gebura_MeetPupleTear">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……别扯废话。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……结界。还有和我相似的……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">还能坚持吧？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……所谓特色，也没啥差别。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">别慌。越慌越忙。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Roland_MeetPupleTear">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……哈。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……要是瞎操心到找上门来，那可太多余了。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……别气馁。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">继续推进。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>