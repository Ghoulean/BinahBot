<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Malkuth</GroupName>
  <Character ID="M_P_Philip">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">已经没什么好说的了吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">……我没有任何恶意。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">请看。你们就像曾经的我一样。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">不过是再次回归而已……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">…………</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">与我不同，你们故步自封，一成不变。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">愿你不会对离开自己身边的人有所留恋……</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Malkuth">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">不要紧张，稳扎稳打地前进吧！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">心情并不怎么舒畅。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……不要放弃。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">没关系，无需自责。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我们能做到的……相信我！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">退无可退了……奋战到底吧！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">你已经很努力了！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">好……就这么走下去吧！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">好像还游刃有余呢？</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Malkuth小姐！现在该怎么办才好……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">结束了……整理好再回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">不……我需要更多援助……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">稍微有些可怕了呀……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">情况僵持不下。再多想想办法……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">除了迎头而上别无选择了吗……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">那些天使的雕像还是老样子。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">得速战速决……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">其他人没事吧？</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">都变了许多呢。我们也是，那家伙也是。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">落幕的味道五味杂陈。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">妈的……可不能就这么结束啊……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">更危险了啊……得多加小心了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">只靠我们自己的力量……能够拦住他吗？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">那个乐团怎么会有如此程度的力量……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">不要再拖延时间了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">毕竟是会过一次的对手……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">感觉还挺顺利的嘛？</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">比起那时陷入暴走的你，应该更难对付了吧……？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">哈啊……真不是个简单的对手……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我不想变成累赘啊……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……看来应该改变战术了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">再这样下去，我们楼层会……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">怎么办……该怎样才好……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">现在还留有余裕呢！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">照这个步调推进就好！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">马上就来帮你！</BattleDialog>
    </Type>
  </Character>
  <Character ID="M_P_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">和过去不同，似乎沉稳了许多。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我们真的赢了吗……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">还得……坚持下去……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">别担心，我不要紧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">抱歉，应该注意你的后背才对。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……余下的就交给我们吧。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">作战方式好像并没有太大的变化……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">又是那种雕像……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">如果整个乐团的力量都达到了这种程度的话……</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>