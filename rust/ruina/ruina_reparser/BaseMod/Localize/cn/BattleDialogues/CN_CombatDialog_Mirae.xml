<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Mirae</GroupName>
  <Character ID="Named">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">事态错综复杂。我们来慢慢揭秘吧？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">最近怪事太多……真叫人头疼。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">我瞧瞧……没什么问题吧？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">情报足够了，回去吧？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">真是的……只可惜这次的条款有些不同。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">就这么，结束了吗……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">卷进了相当麻烦的事件啊……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">有两下子。大伙集中注意力。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我也得先顾好自己。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">对不住啦~我们一向如此。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">还能打探到其他情报……不错。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">哈哈。年纪虽然大了，可本事一点没退步呢。</BattleDialog>
    </Type>
  </Character>

  <Character ID="Mirae">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">事情变大条了……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">总是在做同样的事……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不会有多大麻烦的吧？</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">伊里纳大人，我觉得应该小心为上。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">是啊……据说他们最近升为了都市之星。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">记录完毕，回去吧。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">真是个相当古怪的地方……不知道运用了什么技术……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">最近经常发生无法预测的事件……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">能拿到一本可真是万幸。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">到底……为什么……要来这种地方……</BattleDialog>
      <BattleDialog ID="DEATH_1">我已经……做好觉悟了……</BattleDialog>
      <BattleDialog ID="DEATH_2">所以说这次的委托很危险……</BattleDialog>
      <BattleDialog ID="DEATH_3">罢了……你们还是顾好自己吧……</BattleDialog>
      <BattleDialog ID="DEATH_4">现在发生这种事……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……应该呼叫支援。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">也许委托人所言不虚。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……应该不算太严重吧？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">回去之后要好好整顿……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">光靠我们好像有点难办。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">快点干掉剩下的。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">好像不剩几个了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">他们并不好对付……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">应该有很多值得记录的东西……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">可千万不能混乱。</BattleDialog>
    </Type>
  </Character>
  
  
</BattleDialogRoot>