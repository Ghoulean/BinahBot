<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Tiphereth</GroupName>
  <Character ID="T_O_Oswald">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">喔吼吼！演出开始啦~</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">来来来！无尽的笑声和掌声在哪里！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">感谢陪伴我们演到最后一刻的小丑朋友们……请务必献上热烈的掌声！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">那是只属于我的灯光……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">天天天天呐……我们的团员……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">为什么只让我们发笑！！！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">嘻嘻，这位小丑朋友太~有意思啦！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">兴奋得想要满地打滚吧？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">果然！我们的演出能为大家带来欢乐！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">唱得再大声点儿！</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Tiphereth">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">不要紧。就像往常那样。都明白吗？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……大家辛苦了。整理完毕后再作等待吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……不。我一定要做到。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">还没到瘫倒的时候！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">只要信任彼此，就一定可以……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">不会再让你们为所欲为了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">打起精神来。我们还有希望。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">虽然很累，但再坚持一下！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我一个人也能做到……先操心你自己吧！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">还不赖。大家状态如何？</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">你不会已经在怕了吧？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">嗯……那帮家伙也一定能度过难关吧？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">对不起……已经到极限了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">没关系，剩下的都交给我。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我不会就这么倒下。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……要全力以赴！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">这边搞定啦~</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">虽然很难缠……但确实是个有趣的家伙。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">你们都还好吧？</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">只能……一战了。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">幸好圆满结束了。对不对？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">本该帮上更多的忙……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">越是这种时候……越要冷静啊。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">长相也好，干的事情也罢，全都令人作呕……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">其他人还能坚持下去吗？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">我也觉得自己像个小丑。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">这边顺利完成了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">那声音震得我头皮发麻……</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">怎么像是踏进了马戏场……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">能给人带来欢笑的演出……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我怎么会……败在一群小丑手里……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">会有更好的解决方案……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">就算非常不安……我也会带着你那份继续战斗的。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">对方不容小觑啊。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">好。就像这样继续前进！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">简直像在……表演一样。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">长得可真磕碜……</BattleDialog>
    </Type>
  </Character>
  <Character ID="T_O_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">跟之前见过的那些家伙差不多。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">Tiphereth小姐，现在该回去吗？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">呃……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">不能再让事态恶化下去。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……这没道理吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">所剩不多了……再坚持一下。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">这种吵死人的演出真不像话。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">那个小丑只会耍嘴皮子。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我特么不想和你闹。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>