<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Workshop</GroupName>
  <Character ID="Named">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">准备好试验了吗？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">压力适度……很好。烟气浓度……适中！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">正好让我活络活络筋骨……太棒了！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">哇吼~！这件新产品一定会大火的！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">我还挺好奇的，书都在哪儿呢？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">败得彻底呢……</BattleDialog>
      <BattleDialog ID="DEATH_1">不……直到现在……才知道……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">越是惊险，就越是刺激！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">现在没法用了吧？你这零件我拿走了！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">别慌！下一回可就不好说了。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">这种技术我还是头一次见！你是怎么做到的?</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">你们真有趣！还能耍什么花的吗？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">好啊，没剩多少了？</BattleDialog>
    </Type>
  </Character>

  <Character ID="Workshop">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">好嘞，没问题！马上办！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">这玩意都还没测试过……应该没问题吧？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">对方好像也跃跃欲试呢。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">只要确认没有问题，就能立马交货。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">让我看看……都装填好了吗？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">辛苦了您内！还要再往里走吗？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">这不是挺好使的吗？可以拿回去交差啦！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">能从这儿搞来什么书呢？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">要是能得到各种情报就好了……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">最好能有其他帮派的信息。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">怎么会……哪里出错了……</BattleDialog>
      <BattleDialog ID="DEATH_1">这不可能……啊啊啊……</BattleDialog>
      <BattleDialog ID="DEATH_2">不不不……我不可能出错！</BattleDialog>
      <BattleDialog ID="DEATH_3">……希望不会失败。</BattleDialog>
      <BattleDialog ID="DEATH_4">我不想就这么结束……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">嗯……挥起来是不是有点重？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">这里还得换别的零件……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">你们知道问题出在哪吗？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">武器性能方面似乎没什么大碍。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">一鼓作气，快点结束吧~</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">用不来好货啊……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">应该能学到点东西吧？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">你们到底藏了多少秘密！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">就要收尾了吗？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">我的选择果然没错！</BattleDialog>
    </Type>
  </Character>
  
</BattleDialogRoot>