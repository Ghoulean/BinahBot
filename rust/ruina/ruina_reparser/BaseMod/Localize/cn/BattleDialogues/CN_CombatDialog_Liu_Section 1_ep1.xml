<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>ep1</GroupName>
  <Character ID="Chun">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">冷静地开始吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">绝不能让敌军压制我等。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">对手若是难以撼动，那边转而回防。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">如科长所言，全员不得自乱阵脚。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">百战百胜，不足挂齿。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">虽说巧妙，持久战绝非上策。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……科长大人，恐怕我无以为继了。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">拉长战线只会更加危险。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">前进。不容后撤！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……敌军的态势不容小觑。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">猛虎伏草……即使全员阵亡，我也信任科长大人。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">出其不意，攻其不备……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">千万不要露出破绽。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">坚固的防守，便是守住无法被攻击到的地方。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">若是前途渺茫，又怎会来到这里。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Liu_Fixer_Sec1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">一心向前。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">不露后背！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">化作名誉豪气双收的战场巨龙！</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">无论何时都要身心正直。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">一如既往，始终如一。</BattleDialog>
      <BattleDialog ID="START_BATTLE_5">无所畏惧。冲锋！</BattleDialog>
      <BattleDialog ID="START_BATTLE_6">扫六合，定天下。</BattleDialog>
      <BattleDialog ID="START_BATTLE_7">兵贵神速！</BattleDialog>
      <BattleDialog ID="START_BATTLE_8">椿大人，就这样向前冲吗？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">凭借这种实力走到了今天……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">总之目标达成了，回去吧。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">即使获胜也不得骄傲自满。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">这是他们咎由自取。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">大获全胜。收集好委托的书籍就回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我可不想当个拖后腿的……</BattleDialog>
      <BattleDialog ID="DEATH_1">还不想就这样放弃……</BattleDialog>
      <BattleDialog ID="DEATH_2">无谋……一念之差……</BattleDialog>
      <BattleDialog ID="DEATH_3">犹豫就会落败……</BattleDialog>
      <BattleDialog ID="DEATH_4">已经……到极限了。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">没时间顾及个人感情了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">这种情况下绝不能崩溃。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">人都死了谁能咽下这口气啊。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">继续展开攻势！万万不可坐以待毙。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">临危不惧，永不后退。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">攻敌必所救。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">一鼓作气。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">用安逸的心态战斗只会是这种下场。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">绝不放过一瞬，绝不错漏一人。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">只有手中的武器是值得托付的。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_5">这种攻击是想挠痒痒吗。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Xiao">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">主宰敌军的性命！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">我有克敌制胜之计。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不用管我，若是危险就走为上策。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">感谢各位随我至此。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">胜利了，也并未胜利。海市蜃楼，镜花水月。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">！！！</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……剩下的人一定要保重自己。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">打起精神来！还没有结束……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">我绝不会被击垮……继续前进。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">点到为止！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">精诚所至，金石为开。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">速战速决。全员集中！</BattleDialog>
    </Type>
  </Character>
  <Character ID="Miris">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">逞强只能趁现在啦！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">可别陷入不利战局喽。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">压掉损失，赶紧完事！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">真是万幸……带着书回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">他妈的……太危险了……科长大人！</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……死灰也是会复燃的。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">未竟之役，切忌灰心丧气！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">一定有什么……能够逆转这场战斗。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">科长大人！下一道指示是…！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">在科长大人的统帅下，我们战无不胜！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">所剩无几了！！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">敌军自乱阵脚了，机不可失！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">展开更猛烈的攻势吧！</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>