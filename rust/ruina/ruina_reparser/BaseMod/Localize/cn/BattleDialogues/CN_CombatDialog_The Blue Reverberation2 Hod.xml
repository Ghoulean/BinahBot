<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Hod</GroupName>
  <Character ID="H_G_Greta">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">喀喀喀！那就让咱开始料理吧？！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">是非常有意思的食材啊！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">也不知道其它伙计们的工作做得咋样咯！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">这还真是……别具特色的美味……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">这家伙……不能拿来做菜吗？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">真没劲儿！净是些一口就能吃掉的货色。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">味道不怎么爽口！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">想到新的烹饪技法了！</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Hod">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">一直以来我也通过你们学习到了很多。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……即使如此，哪怕能为大家提供一点帮助………</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我没有大碍……其他人还好吗？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……得找出破局的方法。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">额，嗯。没关系，我不会倒下的！</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">我没想要践踏你的梦想，只是……</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Hod小姐，您还好吧？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……自食恶果。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">不行……越是轻率越会不利。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">还没有完全陷入困境。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">必须看穿她的动作……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……似乎度过了一个难关呢。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">现在可不是担心别人的时候啊。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">无能为力……交给你们了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">只能冷静地……继续前行。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">真是蛮不讲理……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">情况还没那么糟……大概吧……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">万幸的是，我们终于终结了这一切……</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">真担心别的楼层……不知大家是否都相安无事呢……？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我想……我已经尽全力坚持了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">很抱歉。现在能为你做的只有……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">要是一时大意……我也会……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">情况还没有失控……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">无论如何，总算是阻止了她……</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_G_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">这到底有几张嘴呀……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">他妈的……大意了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">慎重看待这次死亡并做出对策吧……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">那头大白鲨……也太蛮横了吧？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">难不成真的是将我们当做食材来宰杀吗……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">哈啊……那把沉甸甸的武器实在是有些吓人。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>