<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Carnival</GroupName>
  <Character ID="Alpha">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">开始吗？看起来不弱，小心吗？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">看起来，很美味吗？但他们，不是人类。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">你们和，清道夫，一样难吃吗？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">吃掉书吗？织出，更美的丝绸吗？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">快点走吗？收到新指令。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……味道，不太好。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">跑太快吗？会被打倒。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">悲伤吗？一起工作的同僚，死了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">不危险吗？还能应付。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">轻易看破招式，很简单吗？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">必须尽快消除，麻烦吗？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我不会让你，打中。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Beta">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">机会来了，能捅到吗？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">看起来很强吗？没问题。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">好奇味道吗？看起来硬，但好像很好吃。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">兴奋吗？胜利，心情愉悦吗？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">我们很强吗？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">再也不能，织布吗？</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">谁被打倒，要想办法吗？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">这样下去，会输吗？要小心。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">必须谨慎吗？或许，有大麻烦。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">吃掉书，织造新面料吗？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">你的力量，不足以在我身上，留下伤疤吗？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">还没结束，剩下几个吗？</BattleDialog>
    </Type>
  </Character>
  <Character ID="Gamma">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">不喜欢受伤吗？不能站着，挨打。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">必须做这个，然后织出新丝绸吗？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">减少不必要动作，节省时间吗？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">未受严重损害吗？疲惫，休息吗？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">剩下交给食指，快速离开吗？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">不能吗？工作尚未，完成……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">变成书吗？死掉就会，变成那样。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">没关系，这样下去，我们会被重创吗？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">随意出手，会有危险吗？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">杀掉一个吗？干净利落。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">觉得我们，能赢吗？理所当然。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">干掉一个，不能掉以轻心吗？</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>