<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Shi Association</GroupName>
  <Character ID="Yujin">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">只有克服危难，才能更上层楼。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">我没事，各位请顾好自己。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不要着急，焦虑会让人斗志尽失。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">但愿能从这些书中找到一丝线索。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">……激烈的战斗，幸存人员请尽快随我撤离。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">离实现夙愿……只差，最后一步……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……默哀片刻。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">你坚持到了最后……谢谢你一直陪伴在我左右。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">胡乱挥舞武器，只会暴露你的弱点。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我从不蔑视死亡，哪怕你是我的敌人。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">还没有彻底结束，各位请认真对待。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Tenma">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">科长，您不用担心，我们会认真辅助您。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">好大的地方……第一次接到这种委托。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">快到极限了……大家请务必保重身体。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……相信我们的科长已经有所安排了。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">竟然把我们派到这种地方……他们到底在密谋什么……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">不，我不能……大家……会累坏的啊……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">我……我会连同你的那份……奋战到底……</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">科长！！！……你们，竟敢……竟敢对柳真科长……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">我不会自乱阵脚……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我还撑得住……我一定能行。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我没事，还有力气。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Valentin">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">他们绝非等闲之辈。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">科长，我们永远追随您。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">我会一如既往地投身战斗。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">这事算是顺利结束了，可我真不甘心……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">不能保证这是最后一次，真叫人不安。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我到极限了……大伙……抱歉……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……我会替你，宰了他们。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">我一定……一定要，亲手斩下这些畜生的头……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">真该庆幸自己还有力气……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">哪怕只剩一人，我也丝毫不会懈怠。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">你的动作，太散漫了。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Shi-Fixers">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">一定要做这种荒唐的事情吗……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">肯定有什么不为人知的阴谋，否则怎会……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">大家别再抱怨了，就像平时那样认真对待吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">有耍嘴皮子的时间，不如看好前方，准备上了。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">大家说得对，这事同样很棘手……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……希望这是最后一件。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">能顺利结束真是值得庆幸。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">我再也没有力气了……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……我就知道……会变成这样。</BattleDialog>
      <BattleDialog ID="DEATH_1">如果大家能养足精神……怎会像现在这样……</BattleDialog>
      <BattleDialog ID="DEATH_2">别管我……大家……专心战斗……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">累坏了吧……你……你辛苦了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……好害怕……我也会变成那样吗……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">大家的状态差到了极点……得尽快结束才行。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">按照预想的那样行动，处理起来还算轻松。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">不知道还能坚持多久……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">……我会负责好我的那部分。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">目标只有一个……快，准，狠。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">从没听说过这种空间，还有这些人……</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>