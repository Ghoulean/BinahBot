<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>HanaAssociation</GroupName>
  <Character ID="Hana_Fixer_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">我活到现在，什么诡异猎奇的星都有见过。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">正如报告所说。图书馆……有种不一样的感觉。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">这些人的服装，我们以前也有见过。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">难道是以这种方式抢夺来宾的力量？</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">总之……以后会经常见面的。请多指教。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">确实相当棘手……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">这些书籍将有助于进一步了解图书馆。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">回去之后有很多报告要写。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">科长她还好吗……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">局势好像稳定住了……回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">大意了……</BattleDialog>
      <BattleDialog ID="DEATH_1">这种结局，还是无法避免吗……</BattleDialog>
      <BattleDialog ID="DEATH_2">只靠我们的力量……果然……</BattleDialog>
      <BattleDialog ID="DEATH_3">最起码要牵制住他们……</BattleDialog>
      <BattleDialog ID="DEATH_4">虽然很不甘心……但现在，是时候放弃了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……我要竭尽全力。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我们会没事的……一定。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……其他人状态如何？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">就快结束了……大家再坚持一下。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">还不算致命。至少目前如此……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">有破绽。看来你急眼了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">不剩几个了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我方也并不轻松。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">我不会失手！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">继续留意四周。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hana_Fixer_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">美里奈小姐，可以开始了吗？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">不用担心。我会妥善处置。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">嗯，我在一旁辅助进攻。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">无论如何……今后还会经常见面。请多指教。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">我会照常进行。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">确实相当棘手……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">似乎并无大碍。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">大家都还好吗？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">这样的委托每次都让人心惊胆战……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">我觉得还行……各位检查一下自己的伤势。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我……不怨谁。</BattleDialog>
      <BattleDialog ID="DEATH_1">我很抱歉……但此刻更要紧的是……</BattleDialog>
      <BattleDialog ID="DEATH_2">只靠我们……果然还是……</BattleDialog>
      <BattleDialog ID="DEATH_3">大意了……本该更谨慎的……</BattleDialog>
      <BattleDialog ID="DEATH_4">是我疏忽了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">对我们还不算致命。至少目前如此……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……我要尽我所能。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我们会没事的……一定会的。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">……其他人状态如何？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">既然无路可退，那就坚持到底。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">有破绽。你很急躁嘛。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">不剩多少了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我们这里也并不轻松。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">不会失手的！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">继续警戒周围。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hana_Mirinae">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">精神集中。尚不知晓对方的真实战力。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">展现你们的成果吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">希望各位充分发挥。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">希望对方不会轻易认输。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">要想得到某物，就该做好失去某物的觉悟……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">保持平常心。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">一旦失败，我们的命运也会就此终结。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我们也并非毫无对策地闯进来。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">没有预测的必要。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">直到最后都不得放松警惕。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">你们才是低估了我们。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hana_Harold">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">距离很近……先观察对方的动向。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">相较其他都市之星，这里并没有什么不同……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">美里奈科长。准备好了吗？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">没什么难的……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">已经成为高悬于夜空的星了吗……</BattleDialog>
      <BattleDialog ID="DEATH_1">吞噬一切的力量……比想象中……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">不得退缩……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">剩下的人都打起精神。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……嗯。这样下去可不妙。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">但愿能像这样顺利解决……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">……就快结束了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">每个人都强得不可小觑……</BattleDialog>
    </Type>
    <Type ID="ERROR" />
  </Character>
