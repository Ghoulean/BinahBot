  <GroupName>Cane Office</GroupName>
  <Character ID="Nemo">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">哈！哈！哈！看来对方可不好对付呢。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">那个那个，咱以后会经常见面的，还请多多关照呀！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">不用担心！咱没道理会输。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">比想象中要磕碜的家伙，还是不少的嘛。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">竟能以如此低价获取情报！咱以后还会经常光顾哟！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">那疯子究竟在盘算什么……这种地方……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">没关系！我还好得很呢。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">哎呀，小心别受伤啊！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">一定要足够谨慎才行！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">动作太慢喽，我眼睛都要看酸啦。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">能掌握奇点技术的秘密……只是时间问题了！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">太简单啦！都再加把劲，干就完了！</BattleDialog>
    </Type>
  </Character>
  <Character ID="Martina">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">老板……请不要太上头……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">……如果当初有仔细留意契约上的内容……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">这种事情我们以前也没少干……不要慌……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">关于W公司的情报……都在这本书里……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">这下可以喘口气了……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">就这么……死了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">辛苦了……抱歉没能拦住他们……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">死后会变成书……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">被逐渐逼进死角了……仔细观察情况……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">为达目的……必须这么做……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">不剩几个了……再努力一下……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">处理好这个，之后还有很多事情要做……</BattleDialog>
    </Type>
  </Character>
  <Character ID="Bada">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">我会照老板吩咐的去做，好好了结此事。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">千万不要冲动行事，冷静地看向前方。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">拿到书后回去就行了，不会有什么难点。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">大家没事吗？确认状态非常重要。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">书由我来保管，快回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">一定要回去……一定要……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……为你默哀，安息吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">是我不够强。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">能多活一人也很重要……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">乌合之众……不难应对。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">根本不费事。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">很快就能清理完毕。</BattleDialog>
    </Type>
