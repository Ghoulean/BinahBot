<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Liu</GroupName>
  <Character ID="Liu-Fixer">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">凝视前方，勇往直前。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">我们绝不背向敌人。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">化身为，光荣豪迈的战场之龙……</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">无论做什么，都要端正心态。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">这跟以往接手的委托并不相似，但一定能以同样的方式处理好。</BattleDialog>
      <BattleDialog ID="START_BATTLE_5">照常发挥就好……不要紧张。</BattleDialog>
      <BattleDialog ID="START_BATTLE_6">我不怕，上吧！</BattleDialog>
      <BattleDialog ID="START_BATTLE_7">我们六协会不会输给任何人。</BattleDialog>
      <BattleDialog ID="START_BATTLE_8">迅速展开攻势。</BattleDialog>
      <BattleDialog ID="START_BATTLE_9">绝不能给大伙添乱。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">这些家伙竟能走到这一步，实在难以置信……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">无论如何，目的都已达成了……大伙快回去吧。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">虽然胜利了，但请不要得意忘形。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">我不会怜悯对手。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_4">顺利取胜，拿上委托要求的书回去吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我讨厌，拖大家后腿……</BattleDialog>
      <BattleDialog ID="DEATH_1">不想就这样……放弃……</BattleDialog>
      <BattleDialog ID="DEATH_2">我大意了……一着不慎全盘皆输……</BattleDialog>
      <BattleDialog ID="DEATH_3">犹疑不定只会成为阻碍……</BattleDialog>
      <BattleDialog ID="DEATH_4">看来……我已经撑到极限了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">没时间顾虑个人情感。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……我们只能默默前进。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">亲眼目睹同事死去，我冷静不了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">……继续展开攻势。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">哪怕危机四伏，我们也决不退缩！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">希望能像方才那样顺利了结。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">以那种心态迎战，这就是你的下场！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我不会错失任何一刻，也不会放跑任何一人。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">现在能依靠的，唯有手中的利刃。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">那种软弱的攻击伤不到我。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Lowell">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">保持专注，这些人不可小觑。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">塞希尔，梅，各位可有准备好？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">决不松懈，我等必将凯旋。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">不要怕。战胜自己，然后战胜敌人。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">各位辛苦了，回收完毕后立刻返回。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">敌方溃不成军，胜利属于我等。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……哪怕结局如此，但我坚持到了最后。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">抱歉……辛苦了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我会继承你的遗志……我们一定会得胜。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……该撤退吗？可现在不是时候……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">情况危急，各位加强防御。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_4">哪怕所有人都已倒下，但只要我一息尚存……就有胜算！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">战斗远没有结束，继续保持专注！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">向前推进，还没有结束……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">哪怕训练有素，总会露出破绽。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Cecil">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">是！准备应战。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">坚定向前，不要露出后背！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">赶在敌人组成阵型之前，先发制人！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我们收集到了不少有利情报。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">不知道1科是否顺利完成工作，我们回去吧？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">以我的实力，要想战胜都市之星……还远远不够吗？</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">继续行动！注意脚下！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我还撑得住，绝不能输……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">端正心态……我们能挺过去。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">动作再狠一些！！</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">干净利落的收尾。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">不剩多少了……一同冲入敌阵！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">热血上涌，状态良好！</BattleDialog>
    </Type>
  </Character>
  <Character ID="Mei">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">准备好了哟！马上出战。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">敌人的数量并不算多嘛！</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">知己知彼！百战不殆！</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我才刚刚热完身耶……真遗憾哟！</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">他们的阵型挥手即破！</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">科长先生，对不起……我本该，坚持下去的……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我不会停下！绝不能辜负死去的同事！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">战斗还……没有结束！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">别叫他们看扁了……我们上！！！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">我不会蠢到连这点心理准备都没有就踏上战场。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">顺利解决喽！</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">大家都没事吧？要知进退哟。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">战斗早就开始了，现在才拿出干劲可太迟了哟！</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>