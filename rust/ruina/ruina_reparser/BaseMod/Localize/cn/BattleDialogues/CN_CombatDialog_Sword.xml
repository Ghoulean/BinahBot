<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Sword</GroupName>
  <Character ID="kimsatgat">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">点灯，启程。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">此地犹如金城汤池……务必攻破。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">一如既往，毅然出剑。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">正所谓同舟共济，吾等理应守望相助。还望来日再会。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">百密一疏……吾难辞其咎。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">刀折矢尽……无能为力。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">……乌云密布，骤雨将临。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">残月照楼台……惜哉，惜哉。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">智者千虑，亦有一失。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">愚昧之辈。谨小慎微亦可置人于死地。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">老马识归途。</BattleDialog>
    </Type>
  </Character>

  <Character ID="sword">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">无论沸水烈火……不遑多让。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">会在此地丧命亦是理所当然。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">呆立之人必败无疑。</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">三下两下即可了结此事。</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">像往常那样出剑。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我会告知锈链。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">他们与我们同船合命……应当给予帮助。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">今天身体格外疲惫……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">铸成大错……</BattleDialog>
      <BattleDialog ID="DEATH_1">可恨……怎能错失这种机会……</BattleDialog>
      <BattleDialog ID="DEATH_2">结果，被剑刃划伤的……是我。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">陷入窘境了……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">时刻将自身置于首位。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">坎坷之行就此结束，愿你安息。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">这家伙想必也打累了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">如此谨慎便无力规避刀剑。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">你不该接下这一击。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>