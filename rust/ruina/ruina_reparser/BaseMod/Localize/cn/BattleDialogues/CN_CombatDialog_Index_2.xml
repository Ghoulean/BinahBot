<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>2</GroupName>
  <Character ID="Esther">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">紧闭双眼，莫被无谓之物动摇心智。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">各守其位，准备迎敌。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">吾辈随时代行指令之意。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……或许，该去见见那位新传令了。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">指令永远指引吾辈迈入正道。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">无可厚非。指令并未要求得胜……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">渴望被赋予意义之人，理应胸怀赴死之决心。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">即刻动身，完成指令。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_2">太阳的高度也会出现偏差。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_3">时刻谨记，麻痹大意将招致死亡。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_4">余下人员继续执行指令。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">无需辩解。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">到此为止。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">……即将完成。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Hubert">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">唯有指令，知晓此地的生死。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">……牵制对手。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">他们的实力无法预测。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">根本不是对手。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">……下一则指令等着我们。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……这便是既定的结局，我全然接受。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">会不满于指令规划的命运吗。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">命运使然。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_2">不要被眼前的事物冲昏头脑。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_3">既定之路，已无法改变。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_4">我们必须完成使命。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">可笑。这便是追随自由意志的结果？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">一气呵成……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">……还算棘手。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Gloria">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">阳也太过分了~怎么把我们送进这种地方呀~！</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">今天也来好好执行指令吧~</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">指令啊~总能把我们引向正确之地呢。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">都解决了，不费吹灰之力哦~</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">指令有说拿走书吗~？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">虽然很伤心~但我无可奈何了呢……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">就这么结束了，真可怜~</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">剩下的人也要继续执行指令才行哟~？</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_2">这也是试炼的一环吗~</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_3">真好奇我的极限在哪里~</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_4">指令不会把我们引向歧途的~</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">你跟刚才那些半吊子一样，笨手又笨脚~</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我没有恶意哦~这也是指令的意思嘛。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">这则指令也快完成了呢~</BattleDialog>
    </Type>
  </Character>
