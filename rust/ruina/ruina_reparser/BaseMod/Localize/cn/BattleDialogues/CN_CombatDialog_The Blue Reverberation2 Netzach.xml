  <Character ID="N_B_Netzach">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">嗯……不试试怎么知道。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">不太……满意。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……这次也没帮上什么忙。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">似乎变危险了……大伙神经再绷紧些。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">大伙都在为生存而奋战吧……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我不会再逃了……</BattleDialog>
