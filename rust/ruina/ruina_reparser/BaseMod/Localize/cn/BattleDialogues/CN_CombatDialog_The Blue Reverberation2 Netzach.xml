  <Character ID="N_B_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">嗯，一路坚持到现在，没道理阻止不了他们吧。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">Netzach先生……其他楼层不要紧吧。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">不妙。再这样下去……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">这真的，不要紧吗……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">现在依然有胜算。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……越来越多了。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">必须把他们全部处理掉。</BattleDialog>
