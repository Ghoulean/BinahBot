<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Hokma</GroupName>
  <Character ID="H_P_Pluto">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">准备完毕了的话……就开始吧。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">最好别被谁人展现的世界所欺骗。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">希望彼此之间不会产生怨恨。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">又一次……被欺骗了吗？</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH" />
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">不要完全相信你的眼睛看见的东西。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">请谅解。我没有恶意。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">我不过是在行我应行之事。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">似乎越来越近了。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Hokma">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">也并非是不可抵挡的洪流。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……只能相信到最后。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">后面就交给其他的人吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">不必自责，安心地睡吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">不应为其所动摇。请各位冷静地平定呼吸。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">我们所有人都已经踏上了无法止步的道路。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">您所言极是，Hokma先生……有什么对策吗？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">说我不气愤……也不过是虚张声势……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">其他人都还好吗？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不必担心我。我很清楚自己的能力。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">要是一蹶不振的话才是真的结束了……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">能平安度过这一劫真是万幸。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">似乎无法通过正常的方式进行战斗……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">对不起，没法再继续帮助你们了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">相信我……然后前进吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">必须得确认一下自己的状态。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我始终相信……情况是会逆转的。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">……真的结束了对吗？</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">要相信自己能赢……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">竟然……耍这种手段。</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">快要结束了……再坚持一会儿……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">所以为什么要如此鲁莽地站出来……！</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">很担心……我们真的能做到吗？</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">好险好险……要是我们没能阻止他的话会发生什么呢。</BattleDialog>
    </Type>
  </Character>
  <Character ID="H_P_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">急功近利会招致祸乱……我会准备好的，</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY" />
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">再这样下去……就危险了啊……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我想要是能再集中一些的话会更好吧。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">将谎言……展现给我看。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">这样会遭殃的……大家都小心点儿。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">也不知道相信这东西到什么程度才算是真相呢。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>