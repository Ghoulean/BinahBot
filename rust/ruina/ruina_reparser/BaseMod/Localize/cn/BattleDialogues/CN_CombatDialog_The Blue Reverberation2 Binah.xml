<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>The Blue Reverberation2 Binah</GroupName>
  <Character ID="B_E_Elena">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">让我看看……你们还留有趋待品尝的血管么？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">忍受干渴的痛苦……并不美妙。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">一场不尽人意的晚宴。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">这份干渴……如何摆脱……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">不过是具空壳罢了。丝血未留……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">死亡是公平的……正与每个人起舞呢。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">被吞噬的命运毫无差别地照耀着我们。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">虽然很可惜……但这不是我想要的味道。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">你们和我都是为满一己私欲而干杯的，不是么？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_4">啊……就凭这也想阻止我？</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Orange">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">……</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Binah">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">不必过度担忧。全神贯注为好。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">看来这里结束了。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">只是能力尚且不足。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">坚持至此已属不易。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">或许你已竭尽全力。辛苦了。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">没有人会斥责你。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">即使只是一具空壳，仍然愿你安息。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">这一次，务必没入永眠。</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">无论如何……既然对方是来宾，就要认真接待。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">辛苦了……有些担心其他楼层的人。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">我绝不后退……！</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我会抗争到底……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">只要还有一个人站着，就没有结束。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">不要着急……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">这样一来，事情能简单许多……</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">那个血魔……也变成书了？</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">居然能操纵特色……可恶，这下岂不是……？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">大家都还好吗？有异常情况请立刻告知。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">抱歉……我撑不下去了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">剩下的人要更加谨慎才行。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不能让迄今为止的努力，化为泡影……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我还撑得住……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">稍慢一拍就出大事了……</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">血魔的力量……都能达到这种程度吗？</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_3">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">朱红……十字。错不了的。那块巨大的十字架……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">……心里真不好受。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">还有敌人……没有……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……大意了吗。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不要贸然行动。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">别在这里停下。</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">哪怕成为空壳，力量也丝毫未减。</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">总感觉头有点晕。</BattleDialog>
    </Type>
  </Character>
  <Character ID="B_E_Librarian_4">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">……她身旁的那个人是谁？</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我都快累死了……慢点整理吧？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">连我也……被打败了……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">还有别的办法吗……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不能因为对方人少就大意轻敌，他们绝非等闲之辈……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">这种局面怎样才能扭转……</BattleDialog>
    </Type>
    <Type ID="SPECIAL_EVENT">
      <BattleDialog ID="SPECIAL_EVENT_0">居然有一天要面对传说中的特色……</BattleDialog>
      <BattleDialog ID="SPECIAL_EVENT_1">真的……不是一般人能够承受的力量。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>