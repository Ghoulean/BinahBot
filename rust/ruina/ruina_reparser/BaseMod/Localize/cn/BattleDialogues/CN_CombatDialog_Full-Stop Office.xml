<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Full-Stop Office</GroupName>
  <Character ID="Liwei">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">大家别紧张，继续向前推进。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">你们提供火力支援，我来挡住他们。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">绝不能打偏……一定要迅速了结他们。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">就算赢了，也不知道能否平安回去……我完全猜不透苍蓝残响的心思。</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">打得好，大家都没事吧？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">是我小瞧了他们……</BattleDialog>
      <BattleDialog ID="DEATH_1">还……还不能……倒下……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……今后该怎么办？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">打起精神！还没有结束。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">这下……难道只能认命了吗？</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">一群莽夫，根本没有预判的必要。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">目标已击毙，瞄准下一个。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">你的死毫无意义。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Tamaki">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">别担心，集中精神战斗，我会好好处理的。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">既然来了，那一定要好好打。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">谨慎虽好，但扣下扳机时不得有一丝犹豫。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">我们的火力，岂是你们能轻易挡住的？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">只要抓准时机，连这种枪械都能有用武之地。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">别开玩笑了！还没有结束……</BattleDialog>
      <BattleDialog ID="DEATH_1">我坚持不住了……抱歉……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">……烦死了，我也不想卷进这种事。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">如果我能更谨慎些……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">阵型千万不能乱……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">进展顺利，再瞄准一些。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">干净利落的收尾。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">不要为自己的无能找借口。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Stephan">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">又来了又来了，又是这种讨人厌的战斗……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">我们冒这么大的风险，究竟是为了什么啊？</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">备弹不剩多少了，完全没有胜算吧……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">这段经历叫人直起鸡皮疙瘩，还好我们赢了……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">就算回得去，苍蓝残响也一定就在那头候着吧？</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">这不公平……我根本就不想进这傻逼地方！</BattleDialog>
      <BattleDialog ID="DEATH_1">都怪那该死的苍蓝傻逼，害我变成这副鸟样！</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">坚持不了多久了……真的还要继续打吗？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">我的精神开始不正常了，别了别了，干脆放弃吧？</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">他妈的……快让我离开这傻逼地方。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">所以说，不要拿自己的小命做赌注。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">好像不剩几个了……？那就这样吧……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">这种鬼地方，我一刻也不想待下去了。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>