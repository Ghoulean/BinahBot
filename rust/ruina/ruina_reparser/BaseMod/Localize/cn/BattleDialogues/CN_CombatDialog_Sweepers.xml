<?xml version="1.0" encoding="utf-8"?>
<BattleDialogRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <GroupName>Sweepers</GroupName>
  <Character ID="Anton">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">我们……现在……开始扫除……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">向前……推进……片刻……不停……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">这片空间……似乎……还有……另一面……</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">现在……家人们……都能……好好吃上饭……</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">希望……邻居们……能再……加把劲……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">尽快……扫完……还有……很多……很多……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">无论谁……倒下……我都要……继续向前……</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">我……没问题……</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">没有……食物……他们……变成了书……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我已经……很久……没有……动手清扫……</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">只要……将拦路的事物……全部……扫除……</BattleDialog>
    </Type>
  </Character>
  <Character ID="Valerie">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">各位邻居，燃料是否充足？</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">和家人一起，必能排除万难。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">为了孩子们，一定要拿到这里的书。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">把掉在那儿的书收回去，嬷嬷一定会高兴的。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">各位邻居……请务必……小心……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">为了留在村里的家人，说什么都不能停下。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">将眼前的一切尽数扫除。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">……有位邻居死了。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">只是像往常一样进餐罢了。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">没有溶化，他们不是人。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">各位，再加把劲。</BattleDialog>
    </Type>
  </Character>
  <Character ID="Lyla">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">家人和邻居都在，我很安心。</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">为了不让嬷嬷生气，我们只能靠自己。</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">扫完就走，我们还要为今晚的清扫做准备。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">这些书会帮到在L巢进餐的家人。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">希望大家……能平安无事……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">扫除时绝对不能分心。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">不用牵挂我们。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">没时间思考了，继续清扫。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">这家伙变成了一本书……没有溶化？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我没问题的，伤口不深。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">还有很多东西需要收拾。</BattleDialog>
    </Type>
  </Character>
  <Character ID="sweepers">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">为了我们的嬷嬷和家人……</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">比起后巷，要清扫的东西并不算多……</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">一心打扫即可。</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">家人的纽带……非常强大……</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">拿上那本书就好了？</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">我们快走吧，夜晚还有事情要做。</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">邻居们……抱……歉……</BattleDialog>
      <BattleDialog ID="DEATH_1">还没有……扫完……</BattleDialog>
      <BattleDialog ID="DEATH_2">家里的孩子，还在等我……</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">我还能继续清扫。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">没关系，我的燃料还有剩余。</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">继续前进。</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">没有溶化？怎么回事？</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">我没事，伤口只有寥寥几道。</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">还有很多事情要做。</BattleDialog>
    </Type>
  </Character>
</BattleDialogRoot>